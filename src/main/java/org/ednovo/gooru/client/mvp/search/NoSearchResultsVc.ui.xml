<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field="noResultStyle" src="no-results.css" type="org.ednovo.gooru.client.mvp.search.NoSearchResultBundle"/>
	<g:HTMLPanel>
	    <div class="{noResultStyle.noResultsBox}">
	    <div class="{noResultStyle.container}">
			<div class="{noResultStyle.noResults}">
				<g:Label styleName="{noResultStyle.header}" ui:field="didnotFindText"></g:Label>
				<g:Label styleName="{noResultStyle.leftColumn}" ui:field="tryText">
					<div class="{noResultStyle.liPosition}"><div class="{noResultStyle.orange}"></div><g:Label styleName="{noResultStyle.liContent}" ui:field="removeFiltersText"></g:Label></div>
					<div class="{noResultStyle.liPosition}"><div class="{noResultStyle.green}"></div><g:Label styleName="{noResultStyle.liContent}" ui:field="checkSpellingText"></g:Label></div>
					<div class="{noResultStyle.liPosition}"><div class="{noResultStyle.lightBlue}"></div><g:Label styleName="{noResultStyle.liContent}" ui:field="differentKeywordText"></g:Label></div>
					<div class="{noResultStyle.liPosition}"><div class="{noResultStyle.blue}"></div><g:Label styleName="{noResultStyle.liContent}" ui:field="changeToggleText"></g:Label></div>
				</g:Label>
				<div class="{noResultStyle.rightColumn}"></div>
			</div>
			
				<g:HTMLPanel styleName="{noResultStyle.suggestedResources}" ui:field="suggestedResourcesContainer">
				   <g:Label styleName="{noResultStyle.header}" ui:field="suggestedresourcesText"></g:Label>
				    <g:Label styleName="{noResultStyle.suggestedCollectionCount}" ui:field="resourcesCount"/>
					
					<!-- <g:HTMLPanel styleName="{noResultStyle.resourceContainer}">
					<div class="{noResultStyle.resourceThumbnail}">
						<div class="{noResultStyle.resourceTypeIcon}"></div>
					</div>
					<div class="{noResultStyle.resourceTitle}">Space Math Worksheets</div>
				</g:HTMLPanel> -->
			
		</g:HTMLPanel>
		</div>
</div>
	    
	</g:HTMLPanel>
</ui:UiBinder> 