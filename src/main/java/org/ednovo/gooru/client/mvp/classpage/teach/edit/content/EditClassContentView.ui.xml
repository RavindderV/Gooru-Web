<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:style src=""></ui:style>
		<g:HTMLPanel styleName="geryBackgroundBody paddingBlock">
			<g:HTMLPanel styleName="setScoreContainer" ui:field="scoreContainer">
					<g:HTMLPanel styleName="col-md-6 col-sm-6">
						<g:HTMLPanel styleName="setScoreLeft">
							<uc:PPanel ui:field="scorePanel"></uc:PPanel>
							<uc:PPanel styleName="help-block" ui:field="helpPanel"></uc:PPanel>
							<g:HTMLPanel styleName="scoreBox">
							 <g:TextBox styleName="form-control" ui:field="scoreTextBox"></g:TextBox>
							 <g:InlineLabel styleName="scorePercentage">%</g:InlineLabel>
							 <g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
							 <g:Label ui:field="errorLabel"></g:Label>
							</g:HTMLPanel>
						</g:HTMLPanel>
					</g:HTMLPanel>
				<g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
				<g:HTMLPanel styleName="actionButtons">
				<g:Button styleName="primary" ui:field="saveBtn"></g:Button>
				<g:Label ui:field="saveLblText"></g:Label>
				</g:HTMLPanel>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="contentSettingsContainer no-gutter" ui:field="contentSeetingsContainer">
			   <g:HTMLPanel styleName="col-md-5 col-sm-8">
			      <g:HTMLPanel styleName="unitLeft">
			         <uc:PPanel ui:field="unitPanel"></uc:PPanel>
			         <g:HTMLPanel styleName="labelStyle customs-select" ui:field="unitList">
			           <!--  <select>
			               <option>Rates &amp; Ratios</option>
			               <option>Blue cheese with crackers</option>
			               <option>Steak</option>
			               <option>Other</option>
			            </select> -->
			         </g:HTMLPanel>
			         <uc:PPanel ui:field="choseTxtPanel"></uc:PPanel>
			         <uc:PPanel ui:field="noteTxtPanel"></uc:PPanel>
			      </g:HTMLPanel>
			   </g:HTMLPanel>
			   <g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
			   <g:HTMLPanel styleName="contentSettingsTableIndications">
			      <g:HTMLPanel styleName="makeallVisible">
			         <g:HTMLPanel styleName="squaredOne">
			            <g:CheckBox></g:CheckBox>
			            <uc:LabelUc></uc:LabelUc>
			         </g:HTMLPanel>
			         <g:InlineLabel ui:field="markAllLbl"></g:InlineLabel>
			      </g:HTMLPanel>
			      <g:HTMLPanel styleName="visible">
			         <g:HTMLPanel styleName="squaredOne">
			            <uc:LabelUc></uc:LabelUc>
			         </g:HTMLPanel>
			         <g:InlineLabel ui:field="visiblLbl"></g:InlineLabel>
			      </g:HTMLPanel>
			      <g:HTMLPanel styleName="hiddenOpt">
			         <g:HTMLPanel styleName="squaredOne">
			           <g:CheckBox></g:CheckBox>
			            <uc:LabelUc></uc:LabelUc>
			         </g:HTMLPanel>
			         <g:InlineLabel ui:field="hiddenLbl"></g:InlineLabel>
			      </g:HTMLPanel>
			   </g:HTMLPanel>
			   <g:HTMLPanel styleName="table-responsive">
			   <g:HTMLPanel styleName="contentSettingsTable">
			      <g:HTMLPanel styleName="contentTable" ui:field="tableConatiner">
			          <g:HTMLPanel styleName="contentTableRow">
						   <g:HTMLPanel styleName="contentTableCell lessonHeader"><uc:PPanel ui:field="lessonPanel"></uc:PPanel></g:HTMLPanel>
						   <g:HTMLPanel styleName="contentTableCell">
						      <g:HTMLPanel styleName="contentTableInner">
						         <g:HTMLPanel styleName="contentTableCellInner"><uc:PPanel ui:field="collectionTitlePanel"></uc:PPanel></g:HTMLPanel>
						         <g:HTMLPanel styleName="contentTableCellInner"><uc:PPanel ui:field="visiblePanel"></uc:PPanel></g:HTMLPanel>
						      </g:HTMLPanel>
						   </g:HTMLPanel>
						</g:HTMLPanel>
			      </g:HTMLPanel>
			   </g:HTMLPanel>
			</g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>
</ui:UiBinder> 


