<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field="ProfilePageTagStyle" src="profile-page-Tag-style.css"></ui:style>
	
	<g:HTMLPanel styleName="{ProfilePageTagStyle.tagBg}">
	  <g:HTMLPanel styleName="{ProfilePageTagStyle.tagTitle}">
	      <g:Label styleName="{ProfilePageTagStyle.label}" ui:field="tagTitle"></g:Label>
	  </g:HTMLPanel>
	   <g:HTMLPanel styleName="{ProfilePageTagStyle.tagRight}">
	      <g:Label styleName="{ProfilePageTagStyle.label}" ui:field="tagcount"></g:Label>
	  </g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 