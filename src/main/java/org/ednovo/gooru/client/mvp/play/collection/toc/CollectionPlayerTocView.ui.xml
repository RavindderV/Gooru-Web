<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:header="urn:import:org.ednovo.gooru.client.mvp.play.collection.header" xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:style field= "playerStyle" src="collectionplayertoc.css"/>
	<g:HTMLPanel styleName="{playerStyle.nav-toc-wrapperContainer}">
		<g:Label ui:field="resourceCountLabel" styleName="{playerStyle.resourceCountStyle}"></g:Label>
		<g:HTMLPanel styleName="{playerStyle.nav-toc-wrapperContainerMain}">
			<g:Label styleName="{playerStyle.leftArrow}" ui:field="previousButton"></g:Label>
		<g:FlowPanel  styleName="{playerStyle.nav-toc-wrapper}  carousel">	
			<g:FlowPanel ui:field="navgationTocContainer" styleName="{playerStyle.nav-toc-container}">
		</g:FlowPanel>		
		
		</g:FlowPanel>
		<g:Label styleName="{playerStyle.rightArrow}" ui:field="nextButton"></g:Label>
		</g:HTMLPanel>
		   <uc:HTMLEventPanel styleName="{playerStyle.resource-info-bottomImageContainer}" ui:field="hideButton">
		      <g:Label styleName="{playerStyle.resource-info-hideImage}"/>
		      <g:Label styleName="{playerStyle.resource-info-hideText}" ui:field="hideText"></g:Label>
		 </uc:HTMLEventPanel>
	</g:HTMLPanel>
	
</ui:UiBinder>

