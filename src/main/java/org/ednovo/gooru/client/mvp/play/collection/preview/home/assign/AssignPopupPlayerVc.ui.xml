<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:a="urn:import:aj.org.objectweb.asm"
	xmlns:gt="urn:import:com.tractionsoftware.gwt.user.client.ui"  xmlns:gg="urn:import:org.ednovo.gooru.client.uc" xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:with
		type="org.ednovo.gooru.client.mvp.play.collection.preview.home.assign.AssignPopUpCBundle"
		field="res" />
	<g:HTMLPanel styleName="{res.css.assign}">
		<g:HTMLPanel styleName="{res.css.assignContent}">
			<g:HTMLPanel styleName="{res.css.assignHeader}">
			<g:Label ui:field="lblAssignTitle" styleName="{res.css.assignTitle}"/>
				<g:Label ui:field="cancelButton"
					styleName="{res.css.assignCloseMarker} {res.css.assignSprite} {res.css.assignCloseMark}"></g:Label>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{res.css.conent}">
		<g:HTMLPanel styleName="{res.css.loadingImageMainDiv}" ui:field="loadingImageLabel">
		      <g:Label  styleName="{res.css.loadingImageForSelfEdit}" />
		   </g:HTMLPanel>
				  <g:HTMLPanel styleName="{res.css.popupContent}" ui:field="popupContentAssign">
		<g:Label ui:field="lblpopupTitle" styleName="{res.css.popupTitle}"/>		  
        <g:Label styleName="{res.css.popupDesc}" ui:field="assignDes"/>
        <g:HTMLPanel styleName="{res.css.popupcornerBg}">
        <g:TextArea styleName="{res.css.shareTextarea}" ui:field="shareLinkTxtBox">
        
            </g:TextArea>
      </g:HTMLPanel>
      <g:Label styleName="{res.css.url}" ui:field="swithUrlLbl"/>
      <g:Label styleName="{res.css.url}" ui:field="swithToEmbedLbl"/>
      <g:HTMLPanel styleName="{res.css.clear}"></g:HTMLPanel>
      <g:HTMLPanel styleName="{res.css.contentBorder}"></g:HTMLPanel>
      
     <uc:HTMLEventPanel ui:field="htmlLoginPanel">   
   	    <g:Label styleName="{res.css.popupTitle}" ui:field="lblLoginPopupTitle"/>  
        <g:Label styleName="{res.css.popupDesc}" ui:field="lblAssignPopDes"/>
        <g:Button ui:field="gmailButton" styleName="{res.css.gConnectButton}"></g:Button>
    	<g:HTMLPanel styleName="{res.css.inputBlockOuter}">
    	    <g:HTMLPanel styleName="{res.css.divider}">
				<g:Label styleName="{res.css.dividerText}" ui:field="lblOr"/>
			</g:HTMLPanel>
       	<gg:TextBoxWithPlaceholder ui:field="loginTxtBox" setFocus="true"/>
		<gg:TextBoxWithPlaceholder ui:field="passwordTxtBox" password="true" />
        <g:HTMLPanel styleName="{res.css.pwdTxt}"><g:Anchor ui:field="forgotPwd"></g:Anchor></g:HTMLPanel>
      </g:HTMLPanel>
      <g:HTMLPanel styleName="{res.css.btn}">
        <g:Button ui:field="loginButton" styleName="primary {res.css.LoginText}"></g:Button>
        <g:Label ui:field="lblPleaseWait" styleName="{res.css.processing}"></g:Label>
      </g:HTMLPanel>
      <g:HTMLPanel styleName="{res.css.signUp}"><g:Label ui:field="donothaveAccountText" ></g:Label>
      <g:Anchor ui:field="ancSignUp"></g:Anchor>
      </g:HTMLPanel>
    
    </uc:HTMLEventPanel>  
      
    	<uc:HTMLEventPanel ui:field="htmlEvenPanelContainer" styleName="{res.css.heightClasspagecontainer}">
			
	</uc:HTMLEventPanel>
	  
    </g:HTMLPanel>
    
			</g:HTMLPanel>
			
		</g:HTMLPanel>
	</g:HTMLPanel>

</ui:UiBinder> 