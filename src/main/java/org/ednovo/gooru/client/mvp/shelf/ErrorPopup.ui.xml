<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc"
	xmlns:panel="urn:import:org.ednovo.gooru.client.ui">
	<ui:with type="org.ednovo.gooru.client.mvp.shelf.ErrorPopUpCBundle"
		field="res" />
	<g:HTMLPanel>
		<div class="{res.css.errorPopupInnerdiv}">
			<div class="{res.css.errorPopupHeaderBg}">
				<div class="{res.css.errorPopupHeaderTitle}">Oops!</div>
				<div class="{res.css.errorPopupCloseBtnContainer}">
					<panel:HTMLEventPanel ui:field="cancelButton"
						styleName="{res.css.errorPopupBtnSprite} {res.css.errorPopupCloseBtn}"></panel:HTMLEventPanel>
				</div>
			</div>
			<g:Label styleName="{res.css.loginPopupErrorMsg}" ui:field="errorMessage" />
			<div class="{res.css.errorPopupCheckBoxContainer}">
<!-- 				<div class="{res.css.errorPopupCheckBoxControl}">
				</div> -->
				<div class="{res.css.errorPopupCheckBoxDescText}">Questions? Email</div>
				<div class="{res.css.supportMailID}">support@goorulearning.org</div>
			</div>
			<div class="{res.css.clear}"></div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>