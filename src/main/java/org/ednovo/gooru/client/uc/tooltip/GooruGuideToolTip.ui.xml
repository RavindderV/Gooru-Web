<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field= "toolTip" src="ToolTip.css"/>
	<g:FlowPanel styleName="{toolTip.organizeContainer}">
	    <g:HTMLPanel styleName="{toolTip.gooruGuidePopupArrow}"></g:HTMLPanel>
	    <g:FlowPanel styleName="{toolTip.organizePopupTextContainer}">
	        <g:FlowPanel styleName="{toolTip.organizeTextContainer}">
	        	<g:HTMLPanel styleName="{toolTip.organizeText}">New to Gooru?</g:HTMLPanel>
	            <g:HTMLPanel styleName="{toolTip.gooruguideText}">Use the Gooru Guide to learn how to discover, organize, teach and study on Gooru.</g:HTMLPanel>
	             <g:HTMLPanel styleName="{toolTip.organizePopupQuestionText}">Questions?</g:HTMLPanel>
	            <g:HTMLPanel styleName="{toolTip.organizePopupEmailSupport}">Email support@goorulearning.org</g:HTMLPanel>
	        </g:FlowPanel>
	     </g:FlowPanel>
	</g:FlowPanel>  
</ui:UiBinder> 