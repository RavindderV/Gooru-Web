<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field="ProfilePageStyle" src="profile-page-Following-style.css"></ui:style>
	<g:HTMLPanel>
	<g:HTMLPanel styleName="{ProfilePageStyle.profilepageFollowerContentText}">
	    <g:InlineLabel styleName="{ProfilePageStyle.inlineStyle}" ui:field="followersTextMessage"></g:InlineLabel> 
	    <g:InlineLabel styleName="{ProfilePageStyle.inlineStyle}" ui:field="followersTextMessageNormal"></g:InlineLabel>
	</g:HTMLPanel>
	<g:ScrollPanel ui:field="followerScrollPanel" styleName="{ProfilePageStyle.scrollStyle}">
		<g:HTMLPanel styleName="{ProfilePageStyle.profilepageFollwoingContent}" ui:field="followersUserConatiner">
		</g:HTMLPanel>
	</g:ScrollPanel>
	
	</g:HTMLPanel>
</ui:UiBinder> 