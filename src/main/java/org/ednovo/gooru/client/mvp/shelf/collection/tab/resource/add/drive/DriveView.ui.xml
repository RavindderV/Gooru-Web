<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:style field="driveStyle" src="driveStyle.css" type="org.ednovo.gooru.client.mvp.shelf.collection.tab.resource.add.drive.GoogleDriveFilesStyleBundle"/> 
	<g:FlowPanel styleName="{driveStyle.filesConatiner}">
		<g:FlowPanel ui:field="panelDriveBreadCrums"></g:FlowPanel>
		<g:Label ui:field="lblLoading" styleName="{driveStyle.loadingText}"></g:Label>
		<g:HTMLPanel ui:field="panelError" styleName="{driveStyle.pannelError}">
			<g:Label ui:field="lblErrorHeading" styleName="{driveStyle.errorHeading}"></g:Label>
			<g:Label ui:field="lblErrorSubHeading"></g:Label>
		</g:HTMLPanel>
		<g:FlowPanel ui:field="panelFileList">
			
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>