<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:tinyMce="urn:import:org.ednovo.gooru.client.ui"
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>
<ui:with type="org.ednovo.gooru.client.mvp.shelf.collection.tab.resource.item.AddSetupAdvancedCBundle"
		field="res"></ui:with>
	<g:HTMLPanel>
			<div class="{res.css.advancedOptionContainer}">
				<div class="{res.css.setupHeader}">Setup Advanced Options?</div>
		
						<g:HTMLPanel styleName="{res.css.setupBoxes}" ui:field="educationUseAdvancedContainer">
						<g:Anchor ui:field="educationUseAdvancedPnl">Educational Use</g:Anchor>
						</g:HTMLPanel>
		
						<g:HTMLPanel styleName="{res.css.setupBoxes}"  ui:field="momentsOfLearningAdvancedContainer">
						<g:Anchor ui:field="momentsOfLearningAdvancedPnl"> Moments of Learning</g:Anchor>
						</g:HTMLPanel>
		
						<g:HTMLPanel styleName="{res.css.setupBoxes}" ui:field="standardsAdvancedContainer">
						<g:Anchor ui:field="standardsAdvancedPnl"> Standards</g:Anchor>
						</g:HTMLPanel>
		
						<g:HTMLPanel styleName="{res.css.setupBoxes}" ui:field="accessHazardAdvancedContainer">
						<g:Anchor ui:field="accessHazardAdvancedPnl"> Access Hazard</g:Anchor>
						</g:HTMLPanel>
		
						<g:HTMLPanel styleName="{res.css.setupBoxes}" ui:field="mediaFeatureAdvancedContainer">
						<g:Anchor ui:field="mediaFeatureAdvancedPnl"> Media Feature</g:Anchor>
						</g:HTMLPanel>
						
						<g:HTMLPanel styleName="{res.css.setupBoxes}" ui:field="mobileFreindlyAdvancedContainer">
						<g:Anchor ui:field="mobileFreindlyAdvancedPnl"> Mobile Friendly</g:Anchor>
						</g:HTMLPanel>
		
				</div>
	</g:HTMLPanel>
</ui:UiBinder>
