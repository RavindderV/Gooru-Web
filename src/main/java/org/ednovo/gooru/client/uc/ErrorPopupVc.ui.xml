<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
<ui:with type="org.ednovo.gooru.client.uc.UcCBundle" field="res" />
	<g:HTMLPanel>
		<g:PopupPanel ui:field="errorPopUpNew" styleName="{res.css.errorPopUp}">
			<g:FlowPanel styleName="{res.css.inlineBorder}">
				<g:FlowPanel ui:field="captionTitle" styleName="{res.css.captionTitle}">
					<g:HTMLPanel>
						<g:Label styleName="{res.css.headerText}" ui:field="errorPopupHeaderText"></g:Label>
					</g:HTMLPanel>
				</g:FlowPanel>
				<g:FlowPanel styleName="{res.css.firstTextBox}">
					<g:HTMLPanel>
						<g:Label styleName="{res.css.dragMsg}" ui:field="dragText"></g:Label>
						<div class="{res.css.QCFText}">
							<g:Label styleName="{res.css.QuestMsg}" ui:field="questionLabel"></g:Label>
							<g:Label styleName="{res.css.QuestMsg}" ui:field="faqsText"></g:Label>
						</div>
					</g:HTMLPanel>
				</g:FlowPanel>
				<g:FlowPanel styleName="{res.css.secondTextBox}">
					<g:FlowPanel styleName="{res.css.OkCancelDiv}">
						<g:Label styleName="{res.css.okButton}" ui:field="okButton"></g:Label>
						<g:Label styleName="{res.css.cancelButton}" ui:field="cancelButton"></g:Label>
					</g:FlowPanel>
				</g:FlowPanel>
			</g:FlowPanel>
		</g:PopupPanel>

	</g:HTMLPanel>
</ui:UiBinder> 