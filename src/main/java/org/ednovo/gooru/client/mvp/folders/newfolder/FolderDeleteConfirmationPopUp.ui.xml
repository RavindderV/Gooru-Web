<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
<ui:with type="org.ednovo.gooru.client.mvp.shelf.ShelfCBundle" field="res" />
	<g:HTMLPanel>
		<g:FlowPanel styleName="{res.css.deletePopupContainer}">
			<g:FlowPanel styleName="{res.css.confirmMessageContainer}">
			<g:FlowPanel styleName="{res.css.confirmMessageTitle}">
				<g:Label styleName="{res.css.confirmMessages}" text="You are about to delete your&nbsp;" />
				<g:Label styleName="{res.css.resourceIndex}" ui:field="entityLbl" />
			</g:FlowPanel>
				<g:Label styleName="{res.css.confirmMessageDelete}" text="This is permanent action and will delete all of its content." />
				<g:Label styleName="{res.css.confirmMessageOk}" text="Please type 'DELETE' and click Ok." />
				<g:TextBox styleName="{res.css.inlineText}" ui:field="inlineTxtBox" />
			</g:FlowPanel>
			<g:FlowPanel styleName="{res.css.actionField}" ui:field="buttonContainer">
				<gg:BlueButtonUc text="OK" ui:field="okButtonUc" styleName="deleteCollectionDisabledButton" />
				<g:Anchor text="Cancel" ui:field="cancelAnr" styleName="collectionFormCancelButton" />
			</g:FlowPanel>
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 