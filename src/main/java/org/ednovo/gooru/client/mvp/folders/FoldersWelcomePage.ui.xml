<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field="WelcomePage" src="folderwelcomepage.css" />
	<g:HTMLPanel>
		<g:HTMLPanel styleName="{WelcomePage.welcomeToWorkspaceContainer}">
		   <g:HTMLPanel styleName="{WelcomePage.welcomeToWorkspaceText}" ui:field="createLbl">
		   </g:HTMLPanel>
		   <g:HTMLPanel styleName="{WelcomePage.getStartedTextContainer}">
		      <g:HTMLPanel styleName="{WelcomePage.getStartedText}" ui:field="gettingStartedLbl"></g:HTMLPanel>
		      <g:Label styleName="{WelcomePage.getStartedCollection}" ui:field="newCollectionUIHandler"></g:Label>
		   </g:HTMLPanel>
		      	<g:HTMLPanel styleName="{WelcomePage.workspaceTextOptions}">
					<g:HTMLPanel styleName="{WelcomePage.firstDiv}">
						<g:HTMLPanel styleName="{WelcomePage.imgTextContainer}">
							<g:Image styleName="{WelcomePage.imageCss}" height="35"
								width="35" ui:field="roundedOneLbl"/>
							<g:HTMLPanel styleName="{WelcomePage.workspaceTitleText}" ui:field="createTextLbl"></g:HTMLPanel>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="{WelcomePage.workspaceCreateDesc}" ui:field="selectedLbl"></g:HTMLPanel>
						<g:HTMLPanel styleName="{WelcomePage.workspaceThumbnail}">
						    <g:Image styleName="{WelcomePage.workspaceImg}" ui:field="workSpaceLbl"/>
						</g:HTMLPanel>
					</g:HTMLPanel>
					<g:HTMLPanel styleName="{WelcomePage.secondDiv}">
						<g:HTMLPanel styleName="{WelcomePage.imgTextContainerCustomize}">
							<g:Image styleName="{WelcomePage.imageCss}" height="35"
								width="35" ui:field="roundedTwoLbl"/>
							<g:HTMLPanel styleName="{WelcomePage.workspaceTitleText}" ui:field="customizeLbl"></g:HTMLPanel>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="{WelcomePage.workspaceDesc}" ui:field="guideStudentsLbl"></g:HTMLPanel>
						<g:HTMLPanel styleName="{WelcomePage.workspaceThumbnail}"><g:Image styleName="{WelcomePage.customizeIma}" ui:field="narrationLbl"/></g:HTMLPanel>
					</g:HTMLPanel>
					<g:HTMLPanel styleName="{WelcomePage.thirdDiv}">
						<g:HTMLPanel styleName="{WelcomePage.imgTextContainer}">
							<g:Image styleName="{WelcomePage.imageCss}" height="35"
								width="35" ui:field="roundedThreeLbl" />
							<g:HTMLPanel styleName="{WelcomePage.workspaceTitleText}" ui:field="shareLbl"></g:HTMLPanel>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="{WelcomePage.workspaceShareDesc}" ui:field="assignLbl"></g:HTMLPanel>
						<g:HTMLPanel styleName="{WelcomePage.workspaceThumbnail}"><g:Image styleName="{WelcomePage.shareImg}" ui:field="shareAssignLbl"/></g:HTMLPanel>
					</g:HTMLPanel>
				</g:HTMLPanel>
		    <g:HTMLPanel styleName="{WelcomePage.workspaceTextSupport}">
				<g:HTMLPanel styleName="{WelcomePage.needSomeMore}"><g:Label ui:field="needHelpLbl"></g:Label>
				    <g:Anchor ui:field="supportCenterAnr" styleName="{WelcomePage.supportLink}" target="_blank"></g:Anchor>.
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 