<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
	<ui:with type="org.ednovo.gooru.client.uc.UcCBundle" field="res" />
	<g:HTMLPanel styleName="{res.css.container}">
	    <g:HTMLPanel styleName="{res.css.standardsHeader}">
			<g:HTMLPanel styleName="{res.css.standardsTitle}" ui:field="standardsText"></g:HTMLPanel>
			<g:Label ui:field="cancelButton"
				styleName="{res.css.standardsCloseMarker} {res.css.standardsSprite} {res.css.standardsCloseMark}"></g:Label>
		</g:HTMLPanel>
		<g:ScrollPanel ui:field="spanelStandardsPanel" styleName="{res.css.standardsContent}">
			<g:HTMLPanel ui:field="mainHtmlPanel">
			    
			</g:HTMLPanel>
		</g:ScrollPanel>
	</g:HTMLPanel>
</ui:UiBinder> 