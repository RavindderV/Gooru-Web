<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field= "toolTip" src="ToolTip.css"/>
	<g:HTMLPanel styleName="{toolTip.searchorganizeContainer}" ui:field="mainPanel">
	    <g:HTMLPanel styleName="{toolTip.hoverPopupArrow}"></g:HTMLPanel>
	    <g:FlowPanel styleName="{toolTip.searchorganizePopupTextContainer}">
	        <g:FlowPanel styleName="{toolTip.searchOrganizeTextContainer}">
	        	<!-- <g:HTMLPanel styleName="{toolTip.organizeText}">Discover</g:HTMLPanel> -->
	            <g:HTMLPanel styleName="{toolTip.searchOrganizeRemainigText}">Enter any Science, Math, Social Sciences, or English Language Arts keyword or topic you would like to discover.
	            <br/><br/>Examples:<br/>
	              <g:Anchor styleName="{toolTip.searchOrganizeLinkText}" ui:field="solarLbl">Solar system</g:Anchor>,
	              <g:Anchor styleName="{toolTip.searchOrganizeLinkText}" ui:field="exponentsLbl">exponents</g:Anchor>,
	              <g:Anchor styleName="{toolTip.searchOrganizeLinkText}" ui:field="oceansLbl">oceans</g:Anchor>,
	              <g:Anchor styleName="{toolTip.searchOrganizeLinkText}" ui:field="cellsLbl">cells</g:Anchor>
	            </g:HTMLPanel>
	           
	        </g:FlowPanel>
	     </g:FlowPanel>
	</g:HTMLPanel>
    
</ui:UiBinder> 