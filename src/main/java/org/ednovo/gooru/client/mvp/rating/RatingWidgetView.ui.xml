		<g:InlineLabel ui:field="star_4" styleName="star"></g:InlineLabel>
		<g:InlineLabel ui:field="star_5" styleName="star"></g:InlineLabel>
		<g:InlineLabel styleName="">(</g:InlineLabel>
		<g:InlineLabel styleName="{style.ratingCountLabel}" ui:field="ratingCountLabel"></g:InlineLabel>
