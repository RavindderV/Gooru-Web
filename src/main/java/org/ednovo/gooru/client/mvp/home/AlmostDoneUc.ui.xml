<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
xmlns:gg="urn:import:org.ednovo.gooru.client.uc" xmlns:panel="urn:import:org.ednovo.gooru.client.ui">
<ui:with type="org.ednovo.gooru.client.mvp.home.AlmostDoneUcCBundle" field="res" />

<g:HTMLPanel>
	
<g:HTMLPanel styleName="{res.css.registrationPopupContainer}">
  <g:HTMLPanel styleName="{res.css.registrationPopupInnerdiv}">
    <g:HTMLPanel styleName="{res.css.registrationPopupHeaderBg}">
      <g:HTMLPanel styleName="{res.css.registrationPopupHeaderTitle}" ui:field="almostDoneText"></g:HTMLPanel>
      <g:HTMLPanel styleName="{res.css.registrationPopupCloseBtnContainer}">
        <panel:HTMLEventPanel ui:field="cancelButton" styleName="{res.css.registrationPopupBtnSprite} {res.css.registrationPopupCloseBtn}"></panel:HTMLEventPanel>
      </g:HTMLPanel>
    </g:HTMLPanel>
    <g:HTMLPanel styleName="{res.css.registrationPopupDesc}" ui:field="fillOutText"></g:HTMLPanel>
    <g:HTMLPanel styleName="{res.css.registrationPopupInputContainer}">
      <g:HTMLPanel styleName="{res.css.registrationPopupInputText}" ui:field="emailText"></g:HTMLPanel>
      <g:HTMLPanel styleName="{res.css.registrationPopupInputOuterDiv}">
        <g:TextBox ui:field="emailTxtBox" styleName="{res.css.registrationPopupInputControl} {res.css.registrationPopupInputBorderNone}"/>
      </g:HTMLPanel>
    </g:HTMLPanel>
    <g:HTMLPanel styleName="{res.css.registrationPopupInputContainer}">
      <g:HTMLPanel styleName="{res.css.registrationPopupInputText}" ui:field="uNameLbl"></g:HTMLPanel>
      <g:HTMLPanel styleName="{res.css.registrationPopupInputOuterDiv}">
        <g:TextBox ui:field="userNameTxtBox" styleName="{res.css.registrationPopupInputControl}"/>
         <g:Label styleName="{res.css.almostDoneFormInputBottomText}" ui:field="errorMessageForUserNameLbl"></g:Label>
      </g:HTMLPanel>
     
    </g:HTMLPanel>
    
    <g:HTMLPanel styleName="{res.css.registrationPopupInputContainer}">
      	<g:HTMLPanel styleName="{res.css.registrationPopupRoleText}" ui:field="roleText"></g:HTMLPanel>
      <g:HTMLPanel styleName="{res.css.registrationPopupDropdownControl}">
       	 <!-- <div class="{res.css.registrationPopupDropdownDesc}">Student</div>
        <div class="{res.css.registrationPopupDownArrContainer}">
          <div class="{res.css.registrationPopupDownArrSprite}{res.css.registrationPopupDownArr}"></div>
        </div> -->
        <g:ListBox ui:field="roleListBox" styleName="{res.css.registrationPopupDropdownControl} {res.css.registrationPopupInputDropDownText}">
          <!-- <div class="{res.css.registrationPopupDropdownList}">Student</div>
          <div class="{res.css.registrationPopupDropdownList}">Teacher</div> -->
        </g:ListBox>
         <g:Label styleName="{res.css.almostDoneFormInputBottomRoleText}" ui:field="errorMessageForRoleLbl"></g:Label>
      </g:HTMLPanel>
    </g:HTMLPanel>
    
     <g:HTMLPanel styleName="{res.css.registrationLegalMessage}">
     <g:Label ui:field="agreeText"></g:Label>
     	<g:Anchor styleName="{res.css.anchorText}" ui:field="termsAndPolicyAnr"/><br/>
     	 <g:Label ui:field="andText"></g:Label><g:Anchor styleName="{res.css.anchorText}" ui:field="copyRightAnr"/><g:Label ui:field="gooruText"></g:Label> 
     </g:HTMLPanel>
    
    <g:HTMLPanel  styleName="{res.css.registrationPopupButtonCotnainer}">
      <g:HTMLPanel ui:field="buttonContainer" styleName="{res.css.registrationPopupBlueButton}">
        <panel:HTMLEventPanel ui:field="okButton" styleName="{res.css.registrationPopupButtonText}"></panel:HTMLEventPanel>
      </g:HTMLPanel>
    </g:HTMLPanel>
  </g:HTMLPanel>
</g:HTMLPanel>
	
</g:HTMLPanel>


</ui:UiBinder>