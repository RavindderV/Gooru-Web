<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:panel="urn:import:org.ednovo.gooru.client.uc">
	<ui:style field="flagPopUpstyle" src="player-flag-popup.css"/>
	<g:HTMLPanel>
		<div class="{flagPopUpstyle.thankyoupopupwrapper}">
	<div class="{flagPopUpstyle.thankyoupopupinnerwrapper}">
    	<div class="{flagPopUpstyle.playerflagpopupheader}">
        	<g:Label styleName="{flagPopUpstyle.playerflagpopupheadertitle}" ui:field="flagHeaderText"></g:Label>
            <panel:HTMLEventPanel ui:field="closeButton" styleName="{flagPopUpstyle.playerflagpopupheaderCloseBtn}">
        	    <g:Image ui:field="popUpCloseButton"/>
        	</panel:HTMLEventPanel> 
        </div>
        <div class="{flagPopUpstyle.playerflagcontentcontainerThankYou}">
        	<g:Label styleName="{flagPopUpstyle.playerflagcontentthankyou}" ui:field="thanksSubmitText"></g:Label>
             <g:Button ui:field="okButton" styleName="{flagPopUpstyle.playerflagbuttonOk}"/> 
            </div>
</div>
</div>
	</g:HTMLPanel>
</ui:UiBinder> 