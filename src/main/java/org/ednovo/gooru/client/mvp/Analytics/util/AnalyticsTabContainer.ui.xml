<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field="unitStyle" src="tabContainer.css" />
	<g:HTMLPanel>
		<div class="collection_teacher_tab_container">
				<g:HorizontalPanel styleName="{unitStyle.tabs_teacher_Summary}">
				  <g:HTMLPanel styleName="{unitStyle.viewSummaryText}"> <g:Button ui:field="btnScoredQuestions" styleName="primary">Scored Questions</g:Button>  </g:HTMLPanel>
				  <g:HTMLPanel styleName="{unitStyle.viewSummaryText}"> <g:Button ui:field="btnOpenEndedQuestions" styleName="primary">Open-Ended Questions</g:Button>  </g:HTMLPanel>
				  <g:HTMLPanel styleName="{unitStyle.viewSummaryText}"> <g:Button styleName="primary" ui:field="btnCollectionBreakDown">	Collection Breakdown</g:Button>  </g:HTMLPanel>
				</g:HorizontalPanel>
			</div>
	</g:HTMLPanel>
</ui:UiBinder> 