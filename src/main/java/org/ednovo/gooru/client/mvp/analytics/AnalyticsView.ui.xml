<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:header="urn:import:org.ednovo.gooru.client.mvp.play.collection.header"
	xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:style field="unitStyle" src="analytics.css" />
	<g:HTMLPanel styleName="{unitStyle.unitDetails}">
		<div class="{unitStyle.containerTeacher}">
			<div class="col-xs-2 {unitStyle.leftPanel}">
				<hr/>
				<uc:UlPanel styleName="list-unstyled {unitStyle.unitMenu}">
				   <g:VerticalPanel ui:field="unitPanel"></g:VerticalPanel>
			    </uc:UlPanel>  
			    
				<g:HTMLPanel styleName="{unitStyle.uniSetup}">
					<g:Label styleName="{unitStyle.moreUnits}" ui:field="lblMoreUnits">More Units</g:Label>
				</g:HTMLPanel>
			</div>
			<div class="col-xs-10 {unitStyle.rightPanel}">
				<div class="row">
					<div class="col-xs-12">
						<div class="col-xs-2 {unitStyle.unitHeading}">Assignments</div>
						<div class="col-xs-2 {unitStyle.uniRequired}">
							<div class="{unitStyle.bubble}"></div> 
							<g:Label ui:field="requiredLabel" styleName="{unitStyle.bubbleMain}"/>
							 <div class="col-xs-2 {unitStyle.requiredOptionalText}">Required</div> 
						</div>
						<div class="col-xs-2 {unitStyle.uniRequired}">
							<div class="{unitStyle.bubbleStyle}"></div> 
							<g:Label ui:field="optionalLabel" styleName="{unitStyle.bubbleMain}"/>
							<div class="col-xs-2 {unitStyle.requiredOptionalText}">Optional</div> 
						</div>
						<div class="clearfix"></div>
					</div>
				</div>
				<!-- 	This widget Is used to Graph Widget-->
				<g:HTMLPanel styleName="row {unitStyle.generalSet}">
				   <g:HTMLPanel>
				       <g:HTMLPanel styleName="{unitStyle.rowInner}" ui:field="graphWidget" >
				          
				        </g:HTMLPanel>
				      </g:HTMLPanel>
				</g:HTMLPanel>
				<!-- end Graph Widget -->
				
				<!-- load all collections start-->
				<g:HTMLPanel styleName="row {unitStyle.generalSet}">
				   <g:HTMLPanel>
				   	   <div class="{unitStyle.narrowReportText}">Narrow report information by collection and student:</div>
				       <g:ListBox styleName="{unitStyle.filterDropDown}" ui:field="loadCollections">
				  	  </g:ListBox>
				      </g:HTMLPanel>
				</g:HTMLPanel>
				<!-- load all collections end -->
				
				<!-- 	This widget Is used for view assignments-->
				<g:HTMLPanel styleName="row {unitStyle.generalSet} {unitStyle.arrowStyle}">
				   <g:HTMLPanel>
				       <g:HorizontalPanel styleName="{unitStyle.rowInner}">
				          <g:HTMLPanel styleName="{unitStyle.viewSummaryText}"> <g:Label styleName="{unitStyle.viewAssignmentText}">View Assignment</g:Label>  </g:HTMLPanel>
				          <g:HTMLPanel styleName="{unitStyle.viewSummaryText}"> <g:Button ui:field="btnCollectionSummary" styleName="primary">Summary</g:Button> <g:Label ui:field="summaryArrowlbl" styleName="{unitStyle.caretUp}"></g:Label>  </g:HTMLPanel>
				          <g:HTMLPanel styleName="{unitStyle.viewSummaryText}"> <g:Button ui:field="btnCollectionProgress" styleName="primary">Progress</g:Button><g:Label ui:field="progressArrowlbl" styleName="{unitStyle.caretUp}"></g:Label> </g:HTMLPanel>
				          <g:HTMLPanel styleName="{unitStyle.viewSummaryText}"> <g:Button ui:field="btnCollectionResponses"  styleName="primary">Responses</g:Button> <g:Label ui:field="responsesArrowlbl" styleName="{unitStyle.caretUp}"></g:Label>  </g:HTMLPanel>
				        </g:HorizontalPanel> 
				      </g:HTMLPanel>
				</g:HTMLPanel>
				<!-- end view assignments -->
				
				<!-- 	This widget Is used for view assignments-->
				<g:HTMLPanel styleName="row {unitStyle.setBroder}">
				   <g:HTMLPanel>
				      <g:HTMLPanel styleName="{unitStyle.rowInner} {unitStyle.slotWidget}" ui:field="slotWidget" >
				        </g:HTMLPanel>
				      </g:HTMLPanel>
				</g:HTMLPanel>
				<!-- end view assignments -->
				
				
				<g:HTMLPanel styleName="row {unitStyle.setBroder}">
					<div class=" {unitStyle.studentPerformance}">
					<div class="row">
						<div class="{unitStyle.unitHeadingAnalytics}">
						Student Performance <span>Heighlight students with certain score range below.</span>
						</div>
						<div class="{unitStyle.scoreText}">
						Minimum score: <span>85%</span>
						</div>
						<div class="clearfix"></div>
						</div>
					<div class="row">
						  <div class="col-xs-12">
					      <div class="{unitStyle.progress}">
						  <div class="{unitStyle.progress_org}" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width:20%;">
						  </div>
					  	  <div class="{unitStyle.progress_green}" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width:20%;">
					  	  </div>
					      <div class="{unitStyle.progress_blue}" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100">
					      </div>
					</div>
					</div>
					</div>
					
					<div class="row scores">
					<div class="col-xs-6">
					Score Below: <input name="" type="text" class="{unitStyle.form_control}" placeholder="20"/> %
					</div>
					<div class="col-xs-6 text-right">
					Score Above: <input name="" type="text" class="{unitStyle.form_control}" placeholder="20"/> %
					</div>
					<div class="clearfix"></div>
					</div>
					
					<div class="row {unitStyle.scoresTable}">
					<div class="col-xs-6 {unitStyle.org}">
					</div>
					<div class="col-xs-6 {unitStyle.green}">
					</div>
					
					<div class="clearfix"></div>
					</div>
					<div class="row scoresTableList">
					<div class="col-xs-6">
					<ul class="list-unstyled list-inline">
					<li><img src="images/score-img.png"/></li>
					<li>Anna</li>
					<li>score</li>
					<li>Time</li>
					<li>Reaction</li>
					</ul>
					
					<ul class="list-unstyled list-inline">
					<li><img src="images/score-img.png"/></li>
					<li>Anna</li>
					<li>score</li>
					<li>Time</li>
					<li>Reaction</li>
					</ul>
					
					
					<ul class="list-unstyled list-inline">
					<li><img src="images/score-img.png"/></li>
					<li>Anna</li>
					<li>score</li>
					<li>Time</li>
					<li>Reaction</li>
					</ul>
					
					
					<ul class="list-unstyled list-inline">
					<li><img src="images/score-img.png"/></li>
					<li>Anna</li>
					<li>score</li>
					<li>Time</li>
					<li>Reaction</li>
					</ul>
					
					
					<ul class="list-unstyled list-inline">
					<li><img src="images/score-img.png"/></li>
					<li>Anna</li>
					<li>score</li>
					<li>Time</li>
					<li>Reaction</li>
					</ul>
					</div>
					<div class="col-xs-6">
					<ul class="list-unstyled list-inline">
					<li><img src="images/score-img.png"/></li>
					<li>Dave</li>
					<li>score</li>
					<li>Time</li>
					<li>Reaction</li>
					</ul>
					<ul class="list-unstyled list-inline">
					<li><img src="images/score-img.png"/></li>
					<li>Dave</li>
					<li>score</li>
					<li>Time</li>
					<li>Reaction</li>
					</ul>
					<ul class="list-unstyled list-inline">
					<li><img src="images/score-img.png"/></li>
					<li>Dave</li>
					<li>score</li>
					<li>Time</li>
					<li>Reaction</li>
					</ul>
					<ul class="list-unstyled list-inline">
					<li><img src="images/score-img.png"/></li>
					<li>Dave</li>
					<li>score</li>
					<li>Time</li>
					<li>Reaction</li>
					</ul>
					<ul class="list-unstyled list-inline">
					<li><img src="images/score-img.png"/></li>
					<li>Dave</li>
					<li>score</li>
					<li>Time</li>
					<li>Reaction</li>
					</ul>
					</div>
					
					<div class="clearfix"></div>
					</div>
					</div>
				</g:HTMLPanel>
				
				
				<div class="row {unitStyle.personalBtn}">
					<div class="col-xs-12">
						<div class="{unitStyle.rowInner}">
							<p class="text-center"><button type="button" class="primary">Personalize Unit 2 for Students</button></p>
						</div>
					</div>
				</div>
				<g:HTMLPanel ui:field="assignmentContainer"></g:HTMLPanel>
			</div>
			<div class="clearfix"></div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>