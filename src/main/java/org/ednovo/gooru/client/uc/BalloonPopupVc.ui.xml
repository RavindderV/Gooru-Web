<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
<ui:with type="org.ednovo.gooru.client.uc.UcCBundle" field="res" />
	<g:FlowPanel>
		<g:HTMLPanel styleName="{res.css.hoverPopupContainer}" ui:field="popupContainer">
		    <g:HTMLPanel styleName="{res.css.hoverPopupArrow}"></g:HTMLPanel>
		    <g:HTMLPanel styleName="{res.css.hoverPopupTextContainer}">
		        <g:Label ui:field="popupDesc" styleName="{res.css.comingSoonLbl}">Coming soon!</g:Label>
		    </g:HTMLPanel>
		</g:HTMLPanel>
	</g:FlowPanel>
</ui:UiBinder> 