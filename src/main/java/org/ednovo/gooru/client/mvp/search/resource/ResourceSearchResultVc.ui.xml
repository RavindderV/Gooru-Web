<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:uc="urn:import:org.ednovo.gooru.client.uc" xmlns:sea="urn:import:org.ednovo.gooru.client.mvp.search.resource">
   <ui:with type="org.ednovo.gooru.client.mvp.search.resource.ResourceSearchResultCBundle" field="res" />
	<sea:ResourceSearchResultWrapperVc ui:field="wrapperVcr">
		<sea:content>
			<g:FlowPanel styleName="{res.css.resourcePanel}">
				<g:FlowPanel styleName="{res.css.resourceHeaderPanel}">
					<uc:ResourceImageUc ui:field="resourceImageUc" />
					<g:FlowPanel styleName="{res.css.resourceHeaderTextPanel}" ui:field="resourceTitlePanel">
						<uc:HTMLEventPanel styleName="resourceSearchTitle" ui:field="resourceTitleContainer">
						    <g:HTML styleName="{res.css.resourceTitle}" ui:field="lblResourceTitle"></g:HTML>
						    <g:Image altText="Not ipad friendly" title="Not ipad friendly" styleName="{res.css.imgHeight}" ui:field="imgNotFriendly" url="images/mos/ipadFriendly.png" ></g:Image>
						</uc:HTMLEventPanel>
						<g:FlowPanel styleName="{res.css.metaDataPanel}" ui:field="metaDataFloPanel"/>
						<g:FlowPanel styleName="{res.css.standards}" ui:field="standardsFloPanel" />
					</g:FlowPanel>
				</g:FlowPanel>
				 <g:HTML styleName="{res.css.resourceDescription}" ui:field="resourceDescriptionHtml" />  
			</g:FlowPanel>
		</sea:content>
	</sea:ResourceSearchResultWrapperVc>
</ui:UiBinder> 