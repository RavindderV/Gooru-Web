<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:panel="urn:import:org.ednovo.gooru.client.ui">
	<ui:style field="sausdStyleUc" src="sausd.css" type="org.ednovo.gooru.client.mvp.library.sausd.SausdStyleBundle" />
		<panel:HTMLEventPanel styleName="{sausdStyleUc.tabsLi} tabsLi" ui:field="featuredCourse">
	        <g:HTMLPanel styleName="{sausdStyleUc.courseImage} {sausdStyleUc.chemistry}">
	           <g:Image ui:field="featuredCourseImage"/>
	           <g:Image styleName="{sausdStyleUc.contributorImage}" ui:field="contributorImage"/>
	        </g:HTMLPanel>
	        <g:Label styleName="{sausdStyleUc.courseTitle}" ui:field="courseTitle"></g:Label>
	        <g:Label ui:field="courseAuthor"></g:Label>
		</panel:HTMLEventPanel>
</ui:UiBinder>