<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="org.ednovo.gooru.client.mvp.classpages.ClasspageCBundle"
		field="res" />
	<g:FlowPanel ui:field="mainFlowPanel">
		<g:HTMLPanel styleName="{res.css.teachWrapper}">
			<g:HTMLPanel styleName="{res.css.myClassPageImageText}">
				<g:HTMLPanel styleName="{res.css.mainClassPageTextTitle}" ui:field="classPageLbl"></g:HTMLPanel>
				<g:Button ui:field="btnNewClasspage" styleName="primary" />
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{res.css.classpageContainer}"
				ui:field="placeHolderForEmptyTeach">
				<g:HTMLPanel styleName="{res.css.classpageText}" ui:field="teachLbl"></g:HTMLPanel>
				<g:HTMLPanel styleName="{res.css.teachNoClasspageDesc}" ui:field="assignLbl"></g:HTMLPanel>
				<g:HTMLPanel styleName="{res.css.classpageTextOptions}">
					<g:HTMLPanel styleName="{res.css.firstDiv}">
						<g:HTMLPanel styleName="{res.css.imgTextContainer}">
							<g:Image styleName="{res.css.imageCss}" height="35"
								width="35" ui:field="roundOneLbl"/>
							<g:HTMLPanel styleName="{res.css.classpageTitleText}" ui:field="createLbl"></g:HTMLPanel>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="{res.css.classpageCreateDesc}" ui:field="createClasspageLbl"></g:HTMLPanel>
						<g:HTMLPanel styleName="{res.css.classpageResourceThumbnail}">
						    <g:Image styleName="{res.css.classpageImg}" ui:field="addClassPageLbl"/>
						</g:HTMLPanel>
					</g:HTMLPanel>
					<g:HTMLPanel styleName="{res.css.secondDiv}">
						<g:HTMLPanel styleName="{res.css.imgTextContainerCustomize}">
							<g:Image styleName="{res.css.imageCss}" height="35"
								 width="35" ui:field="roundTwoLbl"/>
							<g:HTMLPanel styleName="{res.css.classpageTitleText}" ui:field="assignLblClassPage"></g:HTMLPanel>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="{res.css.classpageDesc}" ui:field="addToAssignLbl"></g:HTMLPanel>
						<g:HTMLPanel styleName="{res.css.classpageResourceThumbnail}"><g:Image ui:field="addAssignImgLbl"/></g:HTMLPanel>
					</g:HTMLPanel>
					<g:HTMLPanel styleName="{res.css.thirdDiv}">
						<g:HTMLPanel styleName="{res.css.imgTextContainer}">
							<g:Image styleName="{res.css.imageCss}" height="35"
								 width="35" ui:field="roundThreeLbl"/>
							<g:HTMLPanel styleName="{res.css.classpageTitleText}" ui:field="shareLbl"></g:HTMLPanel>
						</g:HTMLPanel>
						<g:HTMLPanel styleName="{res.css.classpageShareDesc}" ui:field="shareClassPageText"></g:HTMLPanel>
						<g:HTMLPanel styleName="{res.css.classpageResourceThumbnail}"><g:Image ui:field="shareImageLbl" styleName="{res.css.shareImg}"/></g:HTMLPanel>
					</g:HTMLPanel>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="{res.css.classpageTextSupport}">
				    <g:HTMLPanel styleName="{res.css.classpageFooterTextContainer}">
						<g:HTMLPanel styleName="{}"><g:Label ui:field="needHelpText"></g:Label>
						    <g:Anchor styleName="{res.css.supportLink}" target="_blank" ui:field="supportCenterLbl"></g:Anchor>.
						</g:HTMLPanel>
					</g:HTMLPanel>
				</g:HTMLPanel>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{res.css.loadingImageForClasspage}"
				ui:field="loadingPanel" />
			<g:ScrollPanel ui:field="classPageScrollPanel"
				styleName="{res.css.classPageScrollPanel}">
				<g:HTMLPanel ui:field="classpageListPanel" styleName="{res.css.myClassPageListWrapper}">
				</g:HTMLPanel>
			</g:ScrollPanel>

		</g:HTMLPanel>
	</g:FlowPanel>
</ui:UiBinder> 