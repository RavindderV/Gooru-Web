<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:with type="org.ednovo.gooru.client.mvp.shelf.list.ShelfListCBundle" field="res" />
	<g:FlowPanel>
		<g:FocusPanel ui:field="titleFocPanel" styleName="{res.css.shelfCollectionTitle}">
			<g:FlowPanel>
				<g:FlowPanel ui:field="collectionIcon">
					<g:Label styleName="{res.css.shelfBlueLbl}" ui:field="blueLbl"/>
					<g:Label styleName="{res.css.shelfGreyLbl}" ui:field="greyLbl"/>
				</g:FlowPanel>
				<g:FlowPanel ui:field="folderIcon" styleName="{res.css.myFoldersListIcon}"></g:FlowPanel>
				<!-- <g:Label styleName="{res.css.shelfCollectionTitleText}" ui:field="titleLbl" /> -->
				<g:HTML styleName="{res.css.shelfCollectionTitleText}" ui:field="titleLbl" />
				<g:Button ui:field="myShelfEditButton" styleName="{res.css.myShelfEditButton}">Edit</g:Button>
			</g:FlowPanel>
		</g:FocusPanel>
	
	
	
		<g:DisclosurePanel styleName="{res.css.shelfCollection}" ui:field="disPanel" animationEnabled="true">
			<g:customHeader>
				<g:SimplePanel />
			</g:customHeader>
			<g:FocusPanel ui:field="wrapperFocPanel"   styleName="{res.css.shelfContentWrapperPanelForFolders}" >
				<uc:LabelGlassPanel glassText="" glassStyleName="{res.css.shelfGlassPanel}" topStyleName="{res.css.shelfGlassTopPanel}" ui:field="glassContainer">
					<uc:glassContent>
						<g:FlowPanel>
							<g:Label ui:field="addSuccessMsg"></g:Label>
							<g:SimplePanel   styleName="{res.css.shelfContentPanelForFolders}" >
								<g:VerticalPanel ui:field="contentVerPanel"></g:VerticalPanel>
							</g:SimplePanel>
						</g:FlowPanel>
					</uc:glassContent>
				</uc:LabelGlassPanel>
			</g:FocusPanel>
		</g:DisclosurePanel>
	</g:FlowPanel>
</ui:UiBinder>