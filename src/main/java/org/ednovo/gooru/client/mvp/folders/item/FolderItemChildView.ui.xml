<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc"
	xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:with type="org.ednovo.gooru.client.mvp.folders.item.FolderItemCBundle"
		field="res" />
	<g:FlowPanel>
		<g:FlowPanel>
			<g:HTMLPanel styleName="{res.css.myFolderPageNewAddedWrapper}"
				ui:field="folderItemPanel">
				<g:HTMLPanel ui:field="folderIcon" styleName="{res.css.myFoldersIcon}"></g:HTMLPanel>
				<g:HTMLPanel ui:field="collectionIcon">
					<g:Label styleName="{res.css.shelfBlueLbl}" />
					<g:Label styleName="{res.css.shelfGreyLbl}" />
				</g:HTMLPanel>
				<g:HTMLPanel styleName="{res.css.myFolderPageNewAddedTitle}">
					<g:Label ui:field="folderTitleLbl"></g:Label>
				</g:HTMLPanel>
				<g:HTMLPanel ui:field="myFolderPageGoals">
					<g:Label ui:field="folderdescriptionLbl"></g:Label>
				</g:HTMLPanel>
				<g:VerticalPanel styleName="{res.css.myFolderPageNewAddedLeftButtonContainer}"
					ui:field="actionVerPanel">
					<g:Label styleName="{res.css.blueButtonOpen}" ui:field="openFolderpageLbl">Open</g:Label>
					<g:Label styleName="{res.css.deleteButton}" ui:field="confirmDeleteLbl"></g:Label>
				</g:VerticalPanel>
			</g:HTMLPanel>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>