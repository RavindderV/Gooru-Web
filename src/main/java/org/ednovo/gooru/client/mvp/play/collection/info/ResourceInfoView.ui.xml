<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:header="urn:import:org.ednovo.gooru.client.mvp.play.collection.header" xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:style field= "playerStyle" src="resourceinfo.css"/>
    <g:HTMLPanel styleName="{playerStyle.resource-info-backgroundContainer}">
        <g:Label styleName="{playerStyle.resource-info-name}" ui:field="resourceInfoText"></g:Label>
	<g:FlowPanel ui:field="resourceInfoContainerPanel" styleName="{playerStyle.resource-info-container}">
      <g:FlowPanel styleName="{playerStyle.resource-info-Wrapper}">
         <g:HTMLPanel styleName="{playerStyle.resource-info-metadata-wrapper}">
	         <g:HTMLPanel styleName="{playerStyle.resource-info-metadata-innerwrapper}">
	           <g:HTMLPanel styleName="{playerStyle.resource-info-type-image}">
	          	   	<g:Label styleName="{playerStyle.resource-info-title-view}" ui:field="resourceView"> </g:Label>
	           		<g:HTML ui:field="resourceInfoSeparator"/>
	           		<g:Label styleName="{playerStyle.resource-info-title}" ui:field="lblresourceType"/>
	           		<g:Label styleName="{playerStyle.resource-image}" ui:field="resourceTypeImage"/>
	           		<g:HTMLPanel styleName="{playerStyle.ipadFriendlyIconNone}" ui:field="mobileFriendly"></g:HTMLPanel>
	             </g:HTMLPanel>
	           	<g:HTMLPanel styleName="{playerStyle.resource-info-publishContainer}" ui:field="publisherPanel">
	           			 <g:Label styleName="{playerStyle.resource-info-more-title}" ui:field="publisherText"></g:Label>
	              		 <g:Label styleName="{playerStyle.resource-info-title}" ui:field="lblPublisher"/>
	      		</g:HTMLPanel>
	           <g:HTMLPanel ui:field="coursePanel">
		           <g:Label styleName="{playerStyle.resource-info-more-title}" ui:field="courseText"></g:Label>
		           <g:HTMLPanel styleName="{playerStyle.resource-info-title}" ui:field="courseInfo"></g:HTMLPanel>
	          </g:HTMLPanel>
	          <!-- <g:HTMLPanel styleName="{playerStyle.gradeContainer}"> -->
	          <!--  <g:HTMLPanel styleName="{playerStyle.resource-info-grade-wrapper}">
	                        <g:HTMLPanel styleName="{playerStyle.resource-info-grade-content}">
	                        <g:HTMLPanel ui:field="staticGradeText" styleName="{playerStyle.resource-info-grade-text}"/>
	                     </g:HTMLPanel>
	           </g:HTMLPanel> -->
	           <g:HTMLPanel styleName="{playerStyle.resource-info-publishContainer}" ui:field="gradesPanel">
	           			 <g:Label styleName="{playerStyle.resource-info-more-title}" ui:field="gradeTitle"></g:Label>
	              		 <g:Label styleName="{playerStyle.resource-info-title}" ui:field="gradesText"/>
	      		</g:HTMLPanel>
	      <!--  </g:HTMLPanel> -->
	            <g:FlowPanel styleName="{playerStyle.resource-info-more-left-wrapper}" ui:field="licenceContainer">
	               <!-- <g:HTMLPanel styleName="{playerStyle.resource-info-more-leftinner-wrapper}"> -->
	                  <g:HTMLPanel styleName="{playerStyle.resource-info-rights-wrapper}">
	                      <g:Label styleName="{playerStyle.resource-info-more-title}" ui:field="legalText"></g:Label>
	                      <g:HTMLPanel ui:field="rightsLogoContainer" styleName="{playerStyle.resource-info-rights-text}"></g:HTMLPanel>     
	                  </g:HTMLPanel>
	              <!--  </g:HTMLPanel> -->
	            </g:FlowPanel>
	            <g:HTMLPanel styleName="{playerStyle.resource-info-publishContainer}">
	                 <g:Label styleName="{playerStyle.resource-info-more-title}" ui:field="learningobjectiveText" ></g:Label>
	            </g:HTMLPanel>
	            <g:HTMLPanel styleName="{playerStyle.resource-info-description}">
	               <g:HTMLPanel  ui:field="resourceDescription"/>
	            </g:HTMLPanel>
	             <g:HTMLPanel styleName="{playerStyle.resource-info-stand-content}" ui:field="standardsContentContainer">
	               <g:Label styleName="{playerStyle.resource-info-stand-text}" ui:field="standardsText"></g:Label>
	               <g:Label styleName="{playerStyle.resource-info-stand-title}" ui:field="standaInfo"/>
	            </g:HTMLPanel>
	     		<g:FlowPanel ui:field="standardsInfoConatiner" styleName="{playerStyle.resource-info-stand-wrapper}"/>
	  			<g:HTMLPanel styleName="{playerStyle.resource-info-publishContainer}">
	                 <g:Label styleName="{playerStyle.resource-info-more-title}" ui:field="originalUrlTitle" ></g:Label>
	            </g:HTMLPanel>
	            <g:HTMLPanel styleName="{playerStyle.resource-info-description}" ui:field="originalUrlText">
	            </g:HTMLPanel>
	           </g:HTMLPanel>
         </g:HTMLPanel>
         <g:HTMLPanel styleName="{playerStyle.resource-info-collections-wrapper}">
         	 <g:HTMLPanel styleName="{playerStyle.resource-info-collections-count}" ui:field="collectionsText"></g:HTMLPanel>
         	 <g:Label styleName="{playerStyle.resource-info-collections-count}" ui:field="collectionsCount"></g:Label>
         	 <g:ScrollPanel ui:field="scrollPanel" styleName="{playerStyle.playerResourcesRightSection}">
             	<g:HTMLPanel ui:field="reosourceReleatedCollections"></g:HTMLPanel>
             </g:ScrollPanel>
          </g:HTMLPanel>
       </g:FlowPanel>
    </g:FlowPanel>
   <uc:HTMLEventPanel styleName="{playerStyle.resource-info-bottomImageContainer}" ui:field="hideButton">
      <g:Label styleName="{playerStyle.resource-info-hideImage}"/>
      <g:Label styleName="{playerStyle.resource-info-hideText}" ui:field="hideText"></g:Label>
 </uc:HTMLEventPanel>
    </g:HTMLPanel>
</ui:UiBinder>