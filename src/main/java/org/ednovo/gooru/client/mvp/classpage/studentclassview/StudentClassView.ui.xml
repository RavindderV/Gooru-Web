<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:panel="urn:import:org.ednovo.gooru.client.ui"
	xmlns:sg="urn:import:org.ednovo.gooru.client.mvp.home" xmlns:span="urn:import:org.ednovo.gooru.client.uc">
	<ui:style src="" />
	<g:HTMLPanel ui:field="mainContainer">
         <g:HTMLPanel>
            <g:HTMLPanel styleName="bannerInner">
               <g:HTMLPanel styleName="container">
                  <g:HTMLPanel styleName="row">
                     <img src="https://qa.goorulearning.org/images/Classpage/default-classpage.png" class="img-responsive student-banner-image"/>
                     <g:HTMLPanel styleName="student-blueBand">
                        <g:HTMLPanel styleName="col-md-6 col-sm-6 col-md-6-reset">
                           <span:H2Panel ui:field="courseName"/>
                        </g:HTMLPanel>
                        <g:HTMLPanel styleName="col-md-6 col-sm-6 col-md-6-reset">
                           <g:HTMLPanel styleName="rightWhiteBox"><span:SpanPanel ui:field="classCodeSpan"></span:SpanPanel></g:HTMLPanel>
                        </g:HTMLPanel>
                        <g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
                     </g:HTMLPanel>
                  </g:HTMLPanel>
               </g:HTMLPanel>
            </g:HTMLPanel>
            <g:HTMLPanel styleName="tabsContainer">
               <g:HTMLPanel styleName="container">
                  <g:HTMLPanel styleName="row">
                     <g:HTMLPanel styleName="btn-group" ui:field="btnGroup">
                        <g:Button styleName="btn btn-default active" ui:field="learningMapBtn">Learning Map</g:Button>
                        <g:Button styleName="btn btn-default" ui:field="reportsBtn">Reports</g:Button>
                     </g:HTMLPanel>
                  </g:HTMLPanel>
               </g:HTMLPanel>
            </g:HTMLPanel>
            <g:SimplePanel styleName="LearningMapContainer" ui:field="learningMapContainer"/>
         </g:HTMLPanel>
		<sg:FooterUc />
	</g:HTMLPanel>
</ui:UiBinder>