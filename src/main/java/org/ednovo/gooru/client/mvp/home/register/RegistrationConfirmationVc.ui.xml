<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc" >
	<ui:with type="org.ednovo.gooru.client.mvp.home.register.RegisterCBundle" field="res" />
	<g:FlowPanel styleName="{res.css.shelfItemContentDiv}" height="200px">
		<g:Label styleName="{res.css.registerMessageDiv}" ui:field="almostHereText"/>
		<g:Label styleName="{res.css.registerSuccessMessage}" ui:field="sentConformationText"/>
		<g:FlowPanel styleName="{res.css.registerContactMessage}">
			<g:Label ui:field="didnotReceiveText"/>
			<g:Label styleName="{res.css.contactUsText}" ui:field="contactText"/>
			<g:Label styleName="{res.css.supportTextMsg}" ui:field="supportText"/>
		</g:FlowPanel>
		<g:FlowPanel styleName="{res.css.registerFormButtonDiv}">
			<gg:BlueButtonUc ui:field="confirmRegisterUc"/>
		</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder> 