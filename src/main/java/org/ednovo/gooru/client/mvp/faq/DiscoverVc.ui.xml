<?xml version="1.0" encoding="UTF-8"?>
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:eventpanel="urn:import:org.ednovo.gooru.client.ui">
	<!-- <ui:with type="org.ednovo.gooru.client.mvp.faq.GooruGuideCBundle"
		field="gooruGuideStyle"></ui:with> -->
		<ui:style field="GooruGuideStyle" src="GooruGuide.css" />
		
		<g:HTMLPanel styleName="{GooruGuideStyle.popup-bg}">
			<g:HTMLPanel styleName="{GooruGuideStyle.popup}{GooruGuideStyle.gooru-guide}">
				<g:HTMLPanel styleName="{GooruGuideStyle.popup-inner}">
					<g:HTMLPanel styleName="{GooruGuideStyle.popup-content}">
						<g:HTMLPanel styleName="{GooruGuideStyle.guide-container} {GooruGuideStyle.discover}">
							<g:HTMLPanel styleName="{GooruGuideStyle.DOTS-header}" ui:field="discoverText"></g:HTMLPanel>
							<g:HTMLPanel styleName="{GooruGuideStyle.DOTS-description}" ui:field="discoverDescLbl"></g:HTMLPanel>
							<g:HTMLPanel styleName="{GooruGuideStyle.guide-image}" ui:field="gooruGuideImage">
							
								<eventpanel:HTMLEventPanel ui:field="firstId" styleName="{GooruGuideStyle.number}">
									<g:HTMLPanel styleName="{GooruGuideStyle.number-text}" ui:field="gooruGuideStyle_One"></g:HTMLPanel>
									<g:HTMLPanel styleName="{GooruGuideStyle.explanation}" ui:field="Style_One_Desc"></g:HTMLPanel>
								</eventpanel:HTMLEventPanel>
								
								<eventpanel:HTMLEventPanel ui:field="secondId" styleName="{GooruGuideStyle.number}">
									<g:HTMLPanel styleName="{GooruGuideStyle.number-text}" ui:field="gooruGuideStyle_Two"></g:HTMLPanel>
									<g:HTMLPanel styleName="{GooruGuideStyle.explanation}" ui:field="Style_Two_Desc"></g:HTMLPanel>
								</eventpanel:HTMLEventPanel>
								
								<eventpanel:HTMLEventPanel ui:field="thirdId" styleName="{GooruGuideStyle.number}">
									<g:HTMLPanel styleName="{GooruGuideStyle.number-text}" ui:field="gooruGuideStyle_Three"></g:HTMLPanel>
									<g:HTMLPanel styleName="{GooruGuideStyle.explanation}" ui:field="Style_Three_Desc"></g:HTMLPanel>
								</eventpanel:HTMLEventPanel>
								
								<eventpanel:HTMLEventPanel ui:field="fourthId" styleName="{GooruGuideStyle.number}">
									<g:HTMLPanel styleName="{GooruGuideStyle.number-text}" ui:field="gooruGuideStyle_Four"></g:HTMLPanel>
									<g:HTMLPanel styleName="{GooruGuideStyle.explanation}" ui:field="Style_Four_Desc"></g:HTMLPanel>
								</eventpanel:HTMLEventPanel>
								
								<eventpanel:HTMLEventPanel ui:field="fifthId" styleName="{GooruGuideStyle.number}">
									<g:HTMLPanel styleName="{GooruGuideStyle.number-text}" ui:field="gooruGuideStyle_Five"></g:HTMLPanel>
									<g:HTMLPanel styleName="{GooruGuideStyle.explanation}" ui:field="Style_Five_Desc"></g:HTMLPanel>
								</eventpanel:HTMLEventPanel>
							</g:HTMLPanel>
							<g:HTMLPanel styleName="{GooruGuideStyle.supportDiv}"><g:Label ui:field="needHelpLbl"></g:Label><g:Anchor target="_blank" ui:field="supportCenterLbl"></g:Anchor></g:HTMLPanel>
						</g:HTMLPanel>
					</g:HTMLPanel>
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>

</ui:UiBinder>