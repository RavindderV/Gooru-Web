<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field="folderMetaStyle" src="FolderItemMetaDataUc.css"/>
	<g:HTMLPanel styleName="{folderMetaStyle.mainSection} {folderMetaStyle.contentBlock}">
	   <g:HTMLPanel styleName="{folderMetaStyle.ideasBlock}">
	      <g:HTMLPanel styleName="{folderMetaStyle.blockView} {folderMetaStyle.idea}">
	         <g:HTMLPanel styleName="{folderMetaStyle.blockViewTitle}">Big Ideas</g:HTMLPanel>
	         <br/>
	      </g:HTMLPanel>
	      The road to equity is met with many obstacles. 
	   </g:HTMLPanel>
	   <g:HTMLPanel styleName="{folderMetaStyle.ideasBlock}">
	      <g:HTMLPanel styleName="{folderMetaStyle.blockView} {folderMetaStyle.question}">
	         <g:HTMLPanel styleName="{folderMetaStyle.blockViewTitle}">Essential Questions</g:HTMLPanel>
	      </g:HTMLPanel>
	      <br/>
	      +How do different people handle inequality and what does that show about their character?<br/>
	      <br/>
	      +What segregates, or separates people from one another?<br/>
	      <br/>
	      +How has inequality changed over time? 
	   </g:HTMLPanel>
	   <g:HTMLPanel styleName="{folderMetaStyle.ideasBlock} {folderMetaStyle.ideasBlockLast}">
	      <g:HTMLPanel styleName="{folderMetaStyle.blockView} {folderMetaStyle.task}">
	         <g:HTMLPanel styleName="{folderMetaStyle.blockViewTitle}">Performance Task</g:HTMLPanel>
	      </g:HTMLPanel>
	      <br/>
	      Write an argumentative essay
	      Art against racism. 
	   </g:HTMLPanel>
	  <g:Button styleName="{folderMetaStyle.closeItem}" ui:field="closeItem"/>
	</g:HTMLPanel>
</ui:UiBinder> 