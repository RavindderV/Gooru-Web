<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc">
	<ui:with
		type="org.ednovo.gooru.client.mvp.shelf.collection.tab.assign.CollectionAssignCBundle"
		field="res" />
	<g:HTMLPanel styleName="{res.css.assign}">
		<g:HTMLPanel styleName="{res.css.assignContent}">
			<g:HTMLPanel styleName="{res.css.assignHeader}">
				<g:HTMLPanel styleName="{res.css.assignTitle}">Success!</g:HTMLPanel>
				<g:Label ui:field="cancelButton"
					styleName="{res.css.assignCloseMarker} {res.css.assignSprite} {res.css.assignCloseMark}"></g:Label>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{res.css.conent}">
				<g:HTMLPanel styleName="{res.css.assignDesc}">You have successfully assigned</g:HTMLPanel>
				<!-- <g:HTMLPanel styleName="{res.css.assignCollection} {res.css.assignSprite} 
					{res.css.assignCollectionIcon}"></g:HTMLPanel> -->
				<g:Image url="./images/Classpage/collection-image-frame.png" />
				<g:Label ui:field="lblCollectionTitle" styleName="{res.css.assignCollectionDesc}">
				    Exponents Collection</g:Label>
				<g:HTMLPanel styleName="{res.css.assignTo}">to</g:HTMLPanel>
				<g:Label ui:field="lblClasspageTitle" styleName="{res.css.assignClasspageTitle}">Word
					Problems Galore for Algebra I AP</g:Label>
				<g:Label ui:field="lblAssignmentTitle" styleName="{res.css.assignAssignmentTitle}">Word
					Problems Galore for Algebra I AP</g:Label>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 