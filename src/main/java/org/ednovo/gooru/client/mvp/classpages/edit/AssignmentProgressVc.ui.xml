<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"  xmlns:panel="urn:import:org.ednovo.gooru.client.uc">
	<ui:with type="org.ednovo.gooru.client.mvp.classpages.edit.AssignmentProgressCBundle" field="res" />
		<g:FlowPanel ui:field="panelMainContainer" styleName="{res.css.container}">
		    
		    <g:HTMLPanel styleName="{res.css.circleContainer}">
		        <g:Label styleName="{res.css.line}" ui:field="lblLineStart"/>
	 			<g:HTMLPanel styleName="{res.css.circle}" ui:field="panelCircle">
			        <g:HTMLPanel styleName="{res.css.profileVisiblityPopup}">
			          	<g:HTMLPanel styleName="{res.css.bubblearrowstyle}"></g:HTMLPanel>
			          	<g:HTMLPanel styleName=" {res.css.standardsinner} {res.css.standardsPopup}">
							<g:HTML styleName="{res.css.title}" ui:field="htmlCollectiontitle"></g:HTML>
							<g:HTMLPanel styleName="{res.css.moveContainer}">
								<g:Label styleName="{res.css.move}" ui:field="lblMoveTo"></g:Label>
								<g:HTMLPanel styleName="{res.css.myFolderCollectionCategoryDiv}">			
									<g:HTMLPanel styleName="{res.css.myFolderCollectionCategoryInputDiv}">
											<g:HTMLPanel styleName="{res.css.myFolderCollectionCategoryDivText}">
												<g:HTMLPanel>
													<g:Label styleName="{res.css.resourceCategoryLabel}"  ui:field="resourceCategoryLabel" />
												</g:HTMLPanel>
											</g:HTMLPanel>
											<g:HTMLPanel styleName="{res.css.myFolderCollectionArrowleftContainer}">
												<g:Label ui:field="resoureDropDownLbl"	styleName="{res.css.myFolderCollectionPopupSprite} {res.css.myFolderCollectionArrowleft}"></g:Label>
											</g:HTMLPanel>
										</g:HTMLPanel>	
										<g:HTMLPanel styleName="{res.css.reorderLabelContainer}" ui:field="resourceTypePanel">
											<!-- <g:HTMLPanel styleName="{res.css.myFolderCollectionFolderDropdown}"> -->
<!-- 													<g:HTMLPanel styleName="{res.css.myFolderCollectionFolderDropdown} {res.css.myFolderCollectionFolderVideoTitle}" ui:field="videoLabel">3</g:HTMLPanel> -->
											<!-- </g:HTMLPanel> -->
										</g:HTMLPanel>							
						        </g:HTMLPanel>
						        
					        </g:HTMLPanel>					        
			         	</g:HTMLPanel>
			        </g:HTMLPanel>
	 			</g:HTMLPanel>
	 			<g:Label styleName="{res.css.line}" ui:field="lblLineEnd"/>
 			</g:HTMLPanel>
 			<g:Label styleName="{res.css.assignmentNumbers}" ui:field="lblAssignmentNo"/>
 			
		</g:FlowPanel>
</ui:UiBinder> 