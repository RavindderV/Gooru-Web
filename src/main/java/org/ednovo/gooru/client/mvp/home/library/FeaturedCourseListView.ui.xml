<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:panel="urn:import:org.ednovo.gooru.client.ui">
	<ui:style field="libraryStyle" src="library.css"/>
		<panel:HTMLEventPanel styleName="{libraryStyle.tabsLi} tabsLi" ui:field="featuredCourse">
	        <g:HTMLPanel styleName="{libraryStyle.courseImage} {libraryStyle.chemistry}">
	           <g:Image ui:field="featuredCourseImage"/>
	           <g:Image styleName="{libraryStyle.contributorImage}" ui:field="contributorImage"/>
	        </g:HTMLPanel>
	        <g:Label styleName="{libraryStyle.courseTitle}" ui:field="courseTitle"></g:Label>
	        <g:Label ui:field="courseAuthor"></g:Label>
		</panel:HTMLEventPanel>
</ui:UiBinder>