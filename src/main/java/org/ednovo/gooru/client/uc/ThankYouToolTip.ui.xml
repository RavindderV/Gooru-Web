<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:panel="urn:import:org.ednovo.gooru.client.uc">
	<ui:style field="flagPopUpstyle" src="player-flag-popup.css"/>
	<g:HTMLPanel>
		<div class="{flagPopUpstyle.thankyoupopupwrapper}">
	<div class="{flagPopUpstyle.thankyoupopupinnerwrapper}">
    	<div class="{flagPopUpstyle.playerflagpopupheader}">
        	<div class="{flagPopUpstyle.playerflagpopupheadertitle}">Flag</div>
            <panel:HTMLEventPanel ui:field="closeButton" styleName="{flagPopUpstyle.playerflagpopupheaderCloseBtn}">
        	    <g:Image altText="Close" ui:field="popUpCloseButton"/>
        	</panel:HTMLEventPanel> 
        </div>
        <div class="{flagPopUpstyle.playerflagcontentcontainer}">
        	<div class="{flagPopUpstyle.playerflagcontentthankyou}">Thank you for submitting!</div>
             <g:Button styleName="{flagPopUpstyle.playerflagbuttonOk}" text="Ok" ui:field="okButton"/> 
           </div>
</div>
</div>
	</g:HTMLPanel>
</ui:UiBinder> 