<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:panel="urn:import:org.ednovo.gooru.client.ui">
	<ui:style field="toolTip" src="ToolTip.css" />
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
	</ui:style>
	<g:HTMLPanel styleName="{toolTip.tooltipContainer}"
		ui:field="panelCode">
		<!-- <g:HTMLPanel styleName="{toolTip.arrowBorder}"></g:HTMLPanel> -->
		<g:HTMLPanel styleName="{toolTip.discoverTooltipContent}" ui:field="tooltipPanel">
			<g:Label styleName="{toolTip.discoverTooltipLabel}" ui:field="lblGooruLibrary" />
			<g:Label styleName="{toolTip.discoverTooltipLabel}" ui:field="lblRusdLibrary" />
 			<panel:HTMLEventPanel styleName="{toolTip.discoverTooltipLabel}" ui:field="lblPartnerLibrary"/>
			<g:HTMLPanel ui:field="dropdownImg" styleName="{toolTip.partnerDropDownArrow}"></g:HTMLPanel>
		</g:HTMLPanel>
 		<panel:HTMLEventPanel styleName="{toolTip.partnerLibraryContainer}" ui:field="partnerLibContainer"/>
	</g:HTMLPanel>
</ui:UiBinder>