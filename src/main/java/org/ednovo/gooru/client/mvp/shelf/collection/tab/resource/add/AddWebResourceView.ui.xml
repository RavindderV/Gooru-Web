          </g:HTMLPanel>
          <g:Label styleName="{res.css.myFolderCollectionFormInputBottomText}" ui:field="mandatoryTitleLbl">Please enter a title.</g:Label>
        </g:HTMLPanel>
        <g:HTMLPanel styleName="{res.css.myFolderCollectionFormContent}">
