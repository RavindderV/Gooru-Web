<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:a="urn:import:aj.org.objectweb.asm"
	xmlns:gt="urn:import:com.tractionsoftware.gwt.user.client.ui" xmlns:uc="urn:import:org.ednovo.gooru.client.uc"
	xmlns:dt="urn:import:com.google.gwt.user.datepicker.client">
	<ui:style field="style" src="collections.css" />
	<g:HTMLPanel styleName="{style.onoffbtns}" ui:field="switchContainer">
		<g:InlineLabel styleName="{style.text-muted}" ui:field="requiredLabel">Required</g:InlineLabel>
		<label class="{style.switchContainer} switch-green">
		 	<g:SimpleCheckBox styleName="switch-input" ui:field="changeAssignmentStatusButton"></g:SimpleCheckBox>
			<g:InlineLabel styleName="switch-label"></g:InlineLabel>
			<g:InlineLabel styleName="switch-handle" ui:field="sequenceNumberLabel"></g:InlineLabel>
		</label> 
		<g:InlineLabel styleName="{style.text-muted}" ui:field="optionalLabel">Optional</g:InlineLabel>
	</g:HTMLPanel>
</ui:UiBinder> 