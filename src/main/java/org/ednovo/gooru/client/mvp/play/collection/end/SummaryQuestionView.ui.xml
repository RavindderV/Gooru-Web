<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:header="urn:import:org.ednovo.gooru.client.mvp.play.collection.header">
	<ui:style field= "collectionSummaryPageStyle" src="collectionend.css"/>
	<g:HTMLPanel styleName="{collectionSummaryPageStyle.collection-summary-wrapper}">
		<g:HTMLPanel styleName="{collectionSummaryPageStyle.collection-summary-thumbnail-wrapper}">
			<g:Label styleName="{collectionSummaryPageStyle.collection-summary-serial-num}" ui:field="questionSerialNum"></g:Label>
			<g:Image styleName="{collectionSummaryPageStyle.collection-summary-thumbnail}" ui:field="questionThumbnail"  url="http://d2evc3ksyfg5cd.cloudfront.net/qalive/f000/0014/0838/slides/thumbnail.jpg" />
		</g:HTMLPanel>
		<g:HTMLPanel styleName="{collectionSummaryPageStyle.collection-summary-content-wrapper}">
			<g:HTML styleName="{collectionSummaryPageStyle.collection-summary-question-text}" ui:field="questionText"></g:HTML>
			<g:HTMLPanel ui:field="questionAnswerContainer"></g:HTMLPanel>			
			<g:HTML styleName="{collectionSummaryPageStyle.collection-summary-explanation-text}" ui:field="questionExplanation"></g:HTML>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>