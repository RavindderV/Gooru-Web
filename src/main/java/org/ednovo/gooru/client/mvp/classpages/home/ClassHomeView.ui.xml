<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:panel="urn:import:org.ednovo.gooru.client.ui" xmlns:txt="urn:import:org.ednovo.gooru.client.uc"
	xmlns:sg="urn:import:org.ednovo.gooru.client.mvp.home">

	<ui:style field="StudyStyle" src="StudyStyles.css" />
	<ui:with
	type="org.ednovo.gooru.client.mvp.classpages.edit.EditClasspageCBundle"
	field="res" />


			
	<g:HTMLPanel styleName="">
		<g:HTMLPanel styleName="{StudyStyle.container} {StudyStyle.clearfix}">
			<g:HTMLPanel styleName="{StudyStyle.startsection}">
				<g:Label styleName="{StudyStyle.title}" ui:field="lblCreateAClass"></g:Label>
				<g:Label styleName="{StudyStyle.subtitle}" ui:field="lblEasyToOrganize"></g:Label>
				<br />
				<g:Button ui:field="btnCreateClass" styleName="primary"></g:Button>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{StudyStyle.startsection} {StudyStyle.fright}">
				<g:HTMLPanel styleName="{StudyStyle.codeBg}">
					<g:Label styleName="{StudyStyle.codeTitle}" ui:field="lblAccessAClass"></g:Label>
					<txt:TextBoxWithPlaceholder styleName="{StudyStyle.classCodeTxtBox}" ui:field="txtCode"/>
					<g:Button styleName="primary" ui:field="btnEnter"></g:Button>
					<g:Button styleName="primary disabled" ui:field="disabledBtn"></g:Button>
				</g:HTMLPanel>
			</g:HTMLPanel>
			
	
			
		</g:HTMLPanel>
		
		<sg:FooterUc />
	</g:HTMLPanel>
</ui:UiBinder>