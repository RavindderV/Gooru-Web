<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:header="urn:import:org.ednovo.gooru.client.mvp.play.collection.header">
	<ui:style field= "playerStyle" src="resourceframebreaker.css"/>
	<g:FlowPanel styleName="{playerStyle.framebreaker-container}">
	      <g:HTMLPanel styleName="{playerStyle.framebreaker-wrapper}">
				<g:HTMLPanel styleName="{playerStyle.framebreaker-resource-container}" ui:field="frameBanner" >
			      <g:HTMLPanel styleName="{playerStyle.framebreaker-resource-wrapper}">
				    <g:HTMLPanel styleName="{playerStyle.framebreaker-resource-thumbnail}">
				      <g:Image width="80px" height="60px" ui:field="resourceThumbnail"/>
				      <g:Label ui:field="resourceCatrgoryIcon" ></g:Label>
				    </g:HTMLPanel>
					<g:HTMLPanel styleName="{playerStyle.framebreaker-resource-content}">
					    <g:HTMLPanel styleName="{playerStyle.framebreaker-resource-title}" ui:field="resourceTitle"></g:HTMLPanel>
					    <g:Label styleName="{playerStyle.framebreaker-resource-attribution}" ui:field="resourceAttribution"></g:Label>
					</g:HTMLPanel>
			      </g:HTMLPanel>
			      <g:HTMLPanel styleName="{playerStyle.framebreaker-resource-url-conatiner}">
					  <g:HTMLPanel styleName="{playerStyle.framebreaker-resource-url-content}">
					    View this resource by clicking this <g:Anchor styleName="{playerStyle.framebreaker-resource-url}" ui:field="resourceUrl" >lovely little link</g:Anchor> 
					  </g:HTMLPanel>
				 </g:HTMLPanel>
		     </g:HTMLPanel>
		</g:HTMLPanel>
	</g:FlowPanel>
</ui:UiBinder>