<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:style src="" />
	<g:HTMLPanel>
		<g:HTMLPanel styleName="classReportMenuBlock no-gutter" ui:field="reportContainer">
			<g:HTMLPanel styleName="col-md-5 col-sm-7">
			   <g:HTMLPanel styleName="classMenuLeft">
			     <ul>
			         <uc:LiPanel>
			             <g:Label ui:field="notePanel"></g:Label>
		             </uc:LiPanel>
			     </ul>
			   </g:HTMLPanel>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="col-md-7 col-sm-5">
				<g:HTMLPanel styleName="classMenuRight">
					<g:HTMLPanel styleName="coureEditActions">
						<g:Image ui:field="studentImage"/><g:Anchor ui:field="studentAnr">Students</g:Anchor>
						<g:Button styleName="primary" ui:field="addStudentBtn"></g:Button>
						<g:Button styleName="primary" ui:field="connectCourseBtn"></g:Button>
						<g:InlineLabel styleName="courseLblStyle" ui:field="courseHeaderLbl"></g:InlineLabel>
						<g:InlineLabel styleName="folderIcon"></g:InlineLabel>
						<g:InlineLabel styleName="classTitle" ui:field="courseTitleLbl">Course Name</g:InlineLabel>
					</g:HTMLPanel>
				</g:HTMLPanel>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="classReportMenuBlock no-gutter" ui:field="noDataPanel">
			<g:HTMLPanel styleName="col-md-5 col-sm-7">
			   <g:HTMLPanel styleName="classMenuLeft">
			     <ul>
			         <uc:LiPanel>
			             <g:Label ui:field="noDataMsg"></g:Label>
		             </uc:LiPanel>
			     </ul>
			   </g:HTMLPanel>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="clearfix"></g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="" ui:field="courseTable"></g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>