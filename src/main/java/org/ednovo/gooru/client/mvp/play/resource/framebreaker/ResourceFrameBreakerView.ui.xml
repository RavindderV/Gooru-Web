<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:header="urn:import:org.ednovo.gooru.client.mvp.play.collection.header">
	<ui:style field="playerStyle" src="resourceframebreaker.css" />
	<g:FlowPanel>
	<g:FlowPanel
		styleName="{playerStyle.framebreaker-container} {playerStyle.framebreaker-wrapper}" ui:field="resoruceFrameBrakerContainer">
		<g:HTMLPanel styleName="{playerStyle.framebreaker-image}">
			<g:Image ui:field="imgFieldTrip" styleName="{playerStyle.resourceImage}" ></g:Image>
			<g:HTMLPanel ui:field="resourceCategory" styleName="{playerStyle.resourceCategoryImage}" ></g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="{playerStyle.margin20Px}">
		    <g:Label ui:field="lblGooruFieldTrip" styleName="h3 {playerStyle.textColorWhite}"/>
		</g:HTMLPanel>
		<g:HTMLPanel>
		    <g:Label ui:field="lblGooruFieldTripDescUnforseen" styleName="h5 {playerStyle.textColorWhite}"/>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="{playerStyle.margin40Px}">
		    <g:Label ui:field="lblGooruFieldTripDescOriginal" styleName="h5 {playerStyle.textColorWhite}"/>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="{playerStyle.margin40Px}">
			<g:Button styleName="primary" ui:field="btnResourceLink" />
		</g:HTMLPanel>
		<g:HTMLPanel>
		    <g:Label ui:field="lblDontForget" styleName="h5 {playerStyle.resourceComeBack}"/>
		</g:HTMLPanel>
		<g:HTMLPanel>
		 	 <g:Label ui:field="supportTip"/>
		 	 <!-- <g:Anchor ui:field="learnMoreLbl" styleName="{playerStyle.learnMoreLbl}"></g:Anchor> -->
		</g:HTMLPanel>
	</g:FlowPanel>
	</g:FlowPanel>
</ui:UiBinder>