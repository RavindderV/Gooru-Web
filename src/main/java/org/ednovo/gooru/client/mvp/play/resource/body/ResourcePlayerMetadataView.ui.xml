<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:header="urn:import:org.ednovo.gooru.client.mvp.play.collection.header">
	<ui:style field= "playerStyle" src="resourceplayermetadata.css"/>
	<g:FlowPanel styleName="{playerStyle.collectionPlayerWrapper}">
		<g:FlowPanel styleName="{playerStyle.resourcePlayerLeftArrowContainer}"> 
			<g:Button styleName="{playerStyle.leftArrowButtonNormal}" ui:field="backwardButton"></g:Button> 
		</g:FlowPanel> 
		<g:FlowPanel styleName="{playerStyle.resourcePlayerRightArrowContainer}"> 
			<g:Button styleName="{playerStyle.rightArrowButtonNormal}" ui:field="forwardButton"></g:Button> 
		</g:FlowPanel>
		<g:FlowPanel styleName="{playerStyle.collectionPlayerContainer}" ui:field="resourceWidgetContainer"></g:FlowPanel> 
	</g:FlowPanel>
</ui:UiBinder>