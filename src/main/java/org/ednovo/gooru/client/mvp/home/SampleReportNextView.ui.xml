<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:panel="urn:import:org.ednovo.gooru.client.ui">
	<ui:with type="org.ednovo.gooru.client.mvp.home.HomeCBundle"
		field="res" />

	<g:HTMLPanel styleName="{res.css.summaryPopupContainer}">
		<g:HTMLPanel styleName="{res.css.popupInner}">
			<g:Label styleName="{res.css.reportPopupHeader}" ui:field="lblHeading"></g:Label>
			<panel:HTMLEventPanel ui:field="cancelButton"
				styleName="{res.css.closeButton}"></panel:HTMLEventPanel>
			<g:FlowPanel styleName="{res.css.summaryPopupContainer}"
				height="100px">
				<g:FlowPanel styleName="{res.css.summaryText}">
					<g:Label ui:field="lblsummaryText" />
				</g:FlowPanel>
				<g:Label ui:field="lblCollectionProgress" styleName="{res.css.lblCollectionProgress}" />
				<g:Label ui:field="lblCollectionProgressDetails" styleName="{res.css.lblCollectionProgressDetails}" />
				<g:HTMLPanel styleName="{res.css.rightImageContainer}">
				<g:Image ui:field="rightImage" />
				</g:HTMLPanel>
				<g:HTMLPanel styleName="{res.css.imageFieldContainer}"
					ui:field="imageContainer">
				<g:Image ui:field="bgImage"></g:Image>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="{res.css.leftImageContainer}">
				<g:Image ui:field="leftImage" />
				</g:HTMLPanel>
			</g:FlowPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>


</ui:UiBinder> 