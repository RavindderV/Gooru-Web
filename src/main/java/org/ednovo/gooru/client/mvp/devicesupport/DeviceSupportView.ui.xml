<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field="deviceSupport" src="devicesupport.css" />
	<g:HTMLPanel styleName="{deviceSupport.topContainer}">
		<g:HTMLPanel styleName="{deviceSupport.container}">
			<g:HTMLPanel styleName="{deviceSupport.deviceHeader}">
				<g:HTMLPanel styleName="{deviceSupport.gooruLogo}"></g:HTMLPanel>
			</g:HTMLPanel>
			<g:HTMLPanel styleName="{deviceSupport.push}"></g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="{deviceSupport.imageContainer}">
			<g:Image styleName="{deviceSupport.mainImg}" ui:field="deviceSupportLbl"/>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 