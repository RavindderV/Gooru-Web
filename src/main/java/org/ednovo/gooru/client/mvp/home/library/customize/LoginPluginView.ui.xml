<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:a="urn:import:aj.org.objectweb.asm"
	xmlns:gt="urn:import:com.tractionsoftware.gwt.user.client.ui"  xmlns:gg="urn:import:org.ednovo.gooru.client.uc" xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:with
		type="org.ednovo.gooru.client.mvp.home.library.assign.AssignPopUpCBundle"
		field="res" />
			<g:HTMLPanel>
	      	  <g:HTMLPanel styleName="{res.css.popupTitleLogin}">Log in to Customize this collection</g:HTMLPanel>
		      <g:Label ui:field="collectionDescription" styleName="{res.css.popupDescLogin}"></g:Label>
		      <g:HTMLPanel styleName="{res.css.inputBlockOuter}">
		       	<gg:TextBoxWithPlaceholder ui:field="loginTxtBox" setFocus="true"/>
				<gg:TextBoxWithPlaceholder ui:field="passwordTxtBox" password="true" />
		        <g:HTMLPanel styleName="{res.css.pwdTxt}"><g:Anchor ui:field="forgotPwd"></g:Anchor></g:HTMLPanel>
		      </g:HTMLPanel>
		      <g:HTMLPanel styleName="{res.css.btn}">
		        <g:Button ui:field="loginButton" styleName="primary  {res.css.LoginText}">Login</g:Button>
		        <g:Label ui:field="lblPleaseWait" styleName="{res.css.processing}"></g:Label>
		      </g:HTMLPanel>
		      <g:HTMLPanel styleName="{res.css.signUp}">Donâ€™t have an account? <g:Anchor ui:field="ancSignUp" href="#">Sign Up here</g:Anchor></g:HTMLPanel>
			</g:HTMLPanel>
</ui:UiBinder> 