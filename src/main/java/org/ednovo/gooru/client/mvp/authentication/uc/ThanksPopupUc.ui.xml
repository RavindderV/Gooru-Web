					<g:Button styleName="primary {res.css.startUsingGooru}" ui:field="btnStartUsingGooru"/>
				</g:HTMLPanel>
				<g:HTMLPanel styleName="{res.css.footer}" ui:field="panelFooter">
					<g:Label styleName="{res.css.h2}" ui:field="lblWhatsNext"/>
					<g:Label ui:field="lblLearnHowTo"/>
