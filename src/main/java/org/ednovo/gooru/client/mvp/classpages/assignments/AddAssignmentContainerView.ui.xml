<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:with
		type="org.ednovo.gooru.client.mvp.classpages.assignments.AddAssignmentContainerCBundle"
		field="res"></ui:with>
	<g:PopupPanel glassEnabled="true" autoHideEnabled="true">
		<g:HTMLPanel styleName="{res.css.assignmentContainerMainPanel}">
			<g:HTMLPanel styleName="{res.css.assignmentContainer}">
				<g:HTMLPanel styleName="{res.css.myFolderCollectionPopup}">
					<g:HTMLPanel styleName="{res.css.assignmentTitleBlackBg}">
						<g:HTMLPanel styleName="{res.css.assignmentTitle}">Add an Assignment</g:HTMLPanel>
					</g:HTMLPanel>
					<g:HTMLPanel styleName="{res.css.assignmentTabContent}">
						<uc:HTMLEventPanel styleName="{res.css.buttonDeSelected}"
							ui:field="assignmentTabButton">
							<g:HTMLPanel styleName="{res.css.assignmentTabTitleBg}">Assignment</g:HTMLPanel>
						</uc:HTMLEventPanel>
					</g:HTMLPanel>
					<g:HTMLPanel ui:field="tabViewContainer">
					</g:HTMLPanel>
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:PopupPanel>
</ui:UiBinder>