<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
<ui:with type="org.ednovo.gooru.client.uc.UcCBundle" field="res" />
	<g:HTMLPanel>
		<g:PopupPanel ui:field="errorPopUpNew" styleName="{res.css.errorPopUp}">
			<g:FlowPanel styleName="{res.css.inlineBorder}">
				<g:FlowPanel ui:field="captionTitle" styleName="{res.css.captionTitle}">
					<g:HTMLPanel>
						<div class="{res.css.headerText}">Oops!</div>
					</g:HTMLPanel>
				</g:FlowPanel>
				<g:FlowPanel styleName="{res.css.firstTextBox}">
					<g:HTMLPanel>
						<div class="{res.css.dragMsg}">Please drag the resource into collection...</div>
						<div class="{res.css.QCFText}">
							<div class="{res.css.QuestMsg}">Questions?</div>
							<div class="{res.css.QuestMsg}">Check the FAQ's</div>
						</div>
					</g:HTMLPanel>
				</g:FlowPanel>
				<g:FlowPanel styleName="{res.css.secondTextBox}">
					<g:FlowPanel styleName="{res.css.OkCancelDiv}">
						<g:Label styleName="{res.css.okButton}" text="OK" ui:field="okButton"></g:Label>
						<g:Label styleName="{res.css.cancelButton}" text="Cancel"></g:Label>
					</g:FlowPanel>
				</g:FlowPanel>
			</g:FlowPanel>
		</g:PopupPanel>

	</g:HTMLPanel>
</ui:UiBinder> 