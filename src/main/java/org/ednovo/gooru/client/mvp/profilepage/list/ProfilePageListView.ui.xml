<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:sl="urn:import:org.ednovo.gooru.client.mvp.shelf.list"
	xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:with type="org.ednovo.gooru.client.mvp.profilepage.list.ShelfListCBundle"
		field="res" />
	<ui:style>
		.shelfPanel {
			float: right;
			height: 97%;
			margin-bottom: -143px;
			min-height: 759px;
			position: fixed;
			right: 0;
			top: 51px;
			width: 311px;
		}
		.shelfPanelForWorkspace {
			height: 97%;
			margin-bottom: -143px;
			min-height: 759px;
			position: fixed;
			left: 62px;
			top: 173px;
			width: 311px;
		}
		.shelfHeader {
			margin: 0px 0 0px;
			float: left;
		}
		
		.collectionScrlPanel {
			overflow-y: hidden !important;
			overflow-x: hidden !important;
		}
	</ui:style>
		<g:ScrollPanel ui:field="collectionListScrollpanel" styleName="{style.collectionScrlPanel}">
			<g:VerticalPanel ui:field="myShelfVerPanel" />
		</g:ScrollPanel>
</ui:UiBinder> 
