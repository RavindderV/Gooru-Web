<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field="landingPageStyle" src="../landing-page.css" />
		<g:HTMLPanel styleName="{landingPageStyle.pageSection} gooruProvides" ui:field="panelHowToUseGooru">
		   <h2><g:Label ui:field="gooruText"></g:Label></h2>
		   <g:Anchor ui:field="backto" href="#" styleName="{landingPageStyle.returnToTop}"></g:Anchor>
		   <g:HTMLPanel styleName="{landingPageStyle.contentBlock}">
		      <div class="{landingPageStyle.aboutDOTS} discover">
		         <div class="{landingPageStyle.dotsImage} {landingPageStyle.discoverImage}"></div>
		         <h3><g:Label ui:field="discoverLbl"></g:Label></h3>
		         <g:Label ui:field="k12Text"></g:Label>
		      </div>
		      <div class="{landingPageStyle.aboutDOTS} organize">
		         <div class="{landingPageStyle.dotsImage} {landingPageStyle.organizeImage}"></div>
		         <h3><g:Label ui:field="organizeLbl"></g:Label></h3>
		         <g:Label ui:field="customizableText"></g:Label>
		      </div>
		      <div class="{landingPageStyle.aboutDOTS} teach">
		         <div class="{landingPageStyle.dotsImage} {landingPageStyle.teachImage}"></div>
		         <h3><g:Label ui:field="teachText"></g:Label></h3>
		         <g:Label ui:field="assignToStudents"></g:Label>
		      </div>
		      <div class="{landingPageStyle.aboutDOTS} study">
		         <div class="{landingPageStyle.dotsImage} {landingPageStyle.studyImage}"></div>
		         <h3><g:Label ui:field="studyText"></g:Label></h3>
		         <g:Label ui:field="completeAssignmentsText"></g:Label>
		      </div>
		      <div class="{landingPageStyle.clearfix} {landingPageStyle.learnMoreDOTS}">
		         <g:Anchor ui:field="youTubePlayerContainer">
		         	<g:Image ui:field="videoIcon" styleName="{landingPageStyle.videoIcon}"/><g:Label ui:field="watchVideoText"></g:Label>
		         </g:Anchor><g:Label ui:field="orText"></g:Label><g:Anchor target="_blank" ui:field="learnmore"></g:Anchor>
		      </div>
		   </g:HTMLPanel>
		</g:HTMLPanel>
</ui:UiBinder> 