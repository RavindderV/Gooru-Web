<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="org.ednovo.gooru.client.mvp.analytics.collectionProgress.CollectionProgressCBundle" field="res" />
	 <g:HTMLPanel>
 	 <g:HTMLPanel styleName="{res.css.maincontainer}">
 	 <g:HTMLPanel>
 	 <g:HTMLPanel styleName="{res.css.summary_title}">Collection Progress</g:HTMLPanel>
 	 <g:HTMLPanel>Analytics-Test Collection (6 resources | 11 questions)</g:HTMLPanel>
 	 </g:HTMLPanel>
 	 <g:HTMLPanel styleName="{res.css.resource_monitor_header_title}">
		      <g:HTMLPanel styleName="{res.css.floatLeft}"><g:InlineLabel styleName="{res.css.correct_legend_one}" ></g:InlineLabel><span class="correct-one">Correct on first attempt</span></g:HTMLPanel>
		      <g:HTMLPanel styleName="{res.css.floatLeft}"><g:InlineLabel styleName="{res.css.correct_legend_other}"></g:InlineLabel><span class="correct-other">Correct after multiple attempts</span></g:HTMLPanel>
		      <g:HTMLPanel styleName="{res.css.floatLeft}"><g:InlineLabel styleName="{res.css.incorrect_legend}"></g:InlineLabel><span class="incorrect-ans">Incorrect</span></g:HTMLPanel>
	 </g:HTMLPanel>
 	 <g:HTMLPanel>
 	 <g:Label styleName="{res.css.narrowreportText}">Narrow report info by collection :</g:Label>
	 <g:ListBox ui:field="filterDropDown" styleName="{res.css.filterDropDown}">
	 </g:ListBox>
	 <g:HTMLPanel styleName="{res.css.htmlpanlProgress}" ui:field="scrollForCollectionProgress">
	 <g:VerticalPanel ui:field="htmlpnlProgress"/>
	 </g:HTMLPanel>
	 </g:HTMLPanel>
	 </g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 