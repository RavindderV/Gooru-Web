<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.collectionImageOne,.collectionImageTwo,.collectionImageThree {
			background: url("images/Collection-Search/icon-sprite.png") no-repeat
				scroll -279px -207px transparent;
			height: 93px;
			width: 275px;
			position: relative;
		}
		
		.collectionImageOne {
			left: 32px;
			z-index: 10;
			top: 90px;
		}
		
		.collectionImageTwo {
			left: 16px;
			opacity: 0.8;
			top: -22px;
			z-index: 9;
		}
		
		.collectionImageThree {
			opacity: 0.5;
			top: -134px;
			z-index: 8;
		}
		
		.collectionDownArrow {
			background: url(images/SearchDragImages/empty-on-collections.png) no-repeat;
			float: left;
			height: 392px;
			left: 16px;
			position: relative;
			width: 243px;
			z-index: 11;
			top: 70px;
		}
		
		.dragHere {
			color: #999999;
			font-size: 16px;
			width: 311px;
			text-align: center;
			clear: both;
			position: relative;
			top: -90px;
		}
		
		.saveThem {
			color: #999999;
			width: 311px;
			text-align: center;
			clear: both;
			position: relative;
			top: -90px;
		}

		.emptyCollMsgHeading{
			color: #999999;
			width: 284px;
			text-align: center;
			clear: both;
			position: relative;
			top: -152px;
			font-size: 12px;
			font-weight: bold;
		}

		.emptyCollMsgSubHeading{
			color: #999999;
			width: 271px;
			text-align: center;
			clear: both;
			position: relative;
			top: -145px;
			font-size: 12px;
		}

	</ui:style>
	<g:FlowPanel>
		<g:SimplePanel styleName="{style.collectionDownArrow}" />
		<g:Label ui:field="emptyCollMsg_1" styleName="{style.emptyCollMsgHeading}" text="" />
		<g:Label ui:field="emptyCollMsg_2" styleName="{style.emptyCollMsgSubHeading}" text="" />
		<!-- <g:SimplePanel styleName="{style.collectionImageOne}" />
		<g:SimplePanel styleName="{style.collectionImageTwo}" />
		<g:SimplePanel styleName="{style.collectionImageThree}" /> -->
		<g:Label styleName="{style.dragHere}" text="" />
		<g:Label styleName="{style.saveThem}" text="" />
	</g:FlowPanel>
</ui:UiBinder> 