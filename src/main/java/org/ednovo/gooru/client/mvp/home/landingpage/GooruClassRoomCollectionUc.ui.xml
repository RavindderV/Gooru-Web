<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field="landingPageStyle" src="../landing-page.css" type="org.ednovo.gooru.client.mvp.home.LandingPageStyleCss" />
    <g:HTMLPanel>
	    <g:Image styleName="{landingPageStyle.methodsCollection}" ui:field="methodsCollection"></g:Image>
		     <g:HTMLPanel styleName="{landingPageStyle.methodsContentDescription}">
		        <g:Label styleName="{landingPageStyle.methodsContentTitle}" ui:field="contentTitle"></g:Label>
		        <g:HTML ui:field="contentDescription"></g:HTML>
		        <g:Label styleName="{landingPageStyle.methodsContentTitle}" ui:field="howToUseTitle"></g:Label>
		        <g:HTMLPanel ui:field="howToUseDescription"></g:HTMLPanel>
		     </g:HTMLPanel>
	     <g:HTMLPanel styleName="{landingPageStyle.clearfix}"></g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>