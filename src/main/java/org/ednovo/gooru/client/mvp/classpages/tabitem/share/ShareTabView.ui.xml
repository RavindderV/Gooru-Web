<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:a="urn:import:aj.org.objectweb.asm"
	xmlns:gt="urn:import:com.tractionsoftware.gwt.user.client.ui" xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:with
		type="org.ednovo.gooru.client.mvp.classpages.tabitem.share.ShareTabViewCBundle"
		field="res" />
	<g:HTMLPanel styleName="{res.css.mainContainer}">
		<g:HTMLPanel styleName="{res.css.container}">
			<g:Label styleName="{res.css.myAssigntmentSharePageTextGray}"
				text="Web Link" />
			<g:HTMLPanel styleName="{res.css.textContainer}">
				<g:TextBox ui:field="shortenUrlTxtBox"
					styleName="{res.css.myAssigntmentSharePageInputContainerTextGray}" />
			</g:HTMLPanel>
			<g:Label styleName="{res.css.shareDesc}"
				text="This link takes students directly to this Classpage." />
		</g:HTMLPanel>
		<g:HTMLPanel styleName="{res.css.clear}"></g:HTMLPanel>
		<g:HTMLPanel styleName="{res.css.container}">
			<g:Label styleName="{res.css.myAssigntmentSharePageTextGray}"
				text="Class Code" />
			<g:HTMLPanel styleName="{res.css.textContainer}">
				<g:TextBox ui:field="classcodeTxtBox"
					styleName="{res.css.myAssigntmentSharePageInputContainerTextGray}" />
			</g:HTMLPanel>
			<g:Label styleName="{res.css.shareDesc}"
				text="Ask students to enter in this code in the Study tab to view this Classpage." />
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 