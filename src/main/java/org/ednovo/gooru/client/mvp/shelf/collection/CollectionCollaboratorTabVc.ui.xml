<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui"
xmlns:uc="urn:import:org.ednovo.gooru.client.uc" xmlns:a="urn:import:aj.org.objectweb.asm">
<ui:with type="org.ednovo.gooru.client.mvp.shelf.collection.CollectionCBundle" field="res" />
	<g:HTMLPanel>
		<g:FlowPanel styleName="{res.css.collaboratorPanel}">
			<g:FlowPanel styleName="{res.css.addCollaborator}">
				<!-- <g:SimplePanel styleName="{res.css.clear}" /> -->
				<g:Label styleName="{res.css.addCollaboratorText}" ui:field="collaboratorsText"/>
				<g:FlowPanel styleName="">
					<g:TextBox styleName="{res.css.collaboratorBox}" ui:field="collaboratorTxtBox"/>
					<g:Label ui:field="gooruBlueLbl" styleName="{res.css.addCollabBlueBtn}"></g:Label>
				</g:FlowPanel>
			</g:FlowPanel>
			<g:FlowPanel styleName="{res.css.addedCollaborator}">
				<!-- <g:SimplePanel styleName="{res.css.clear}" /> -->
				<g:FlowPanel ui:field="addedCollaboratorFloPanel" styleName="{res.css.collabList}"/>	
			</g:FlowPanel>
		</g:FlowPanel>
		<g:FlowPanel styleName="{res.css.collaboratorPanelRight}">
		<g:Label styleName="{res.css.collaboratorRecent}" ui:field="addRecentText"/>
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 