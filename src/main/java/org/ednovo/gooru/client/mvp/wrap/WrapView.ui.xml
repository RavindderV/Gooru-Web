<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.mvp.search" xmlns:sg="urn:import:org.ednovo.gooru.client.mvp.home"
	xmlns:sh="urn:import:org.ednovo.gooru.client.mvp.shelf" xmlns:cs="urn:import:org.ednovo.gooru.client.mvp.search" xmlns:d="urn:import:com.google.gwt.dom.client" xmlns:ps="urn:import:org.ednovo.gooru.client.uc">
	<ui:style>
		.wrapper {
			position: relative;
			min-width: 1024px;
			height: auto !important;
		}
		.wrapperContent {
			margin-top: 36px;
			min-height: 450px;
		}
	</ui:style>
	<g:HTMLPanel styleName="{style.wrapper}" ui:field="panelWrapper">
		<sg:HeaderUc ui:field="headerUc"/>
		<g:SimplePanel ui:field="wrapperPanel" styleName="{style.wrapperContent}" />
		<!-- <sg:FooterUc /> -->
	</g:HTMLPanel>

</ui:UiBinder> 
