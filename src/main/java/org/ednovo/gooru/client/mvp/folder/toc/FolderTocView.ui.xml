<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:html5="urn:import:org.ednovo.gooru.client.htmltags"  xmlns:tbp="urn:import:org.ednovo.gooru.client.uc">
	<ui:style field="tocStyle" src="floderToc.css" />
	<ui:with field="i18n" type="org.ednovo.gooru.shared.i18n.MessageProperties"/>
	<ui:style field="tocStyle1" src="folderStyleToc.css" />
	<g:HTMLPanel>
	 	<g:Hidden ui:field="myHiddenField"/>
		<g:HTMLPanel styleName="{tocStyle.bodyContainer}">
			<g:HTMLPanel styleName="container">
				<g:HTMLPanel styleName="row">
				     <!-- Library banner image start here -->
				    <g:HTMLPanel styleName="{tocStyle1.bannerImageContainer}" ui:field="bannerImagePanel">
							<g:Image  styleName="img-responsive" ui:field="bannerImage"/>
							<g:HTMLPanel styleName="{tocStyle1.bannerImageCaption}">
								<tbp:H2Panel ui:field="bannerTitle"></tbp:H2Panel>
					
								<g:HTMLPanel styleName="{tocStyle1.sublogoContainer}">
									<g:Image  ui:field="logoImage"/>
								</g:HTMLPanel>
							</g:HTMLPanel>
					</g:HTMLPanel>
					<!--  Library banner image ends here -->
					<!--  Profile banner image starts here -->
					<g:HTMLPanel styleName="{tocStyle1.bannerimageContainer1}" ui:field="profileBannerPanel">
						<g:HTMLPanel styleName="{tocStyle1.bannerimageCaption1}">
							  <g:Image styleName="{tocStyle1.imgResponsive} img-circle" ui:field="profImage"/> <tbp:H2Panel ui:field="userTitle"></tbp:H2Panel>
						</g:HTMLPanel>
					</g:HTMLPanel>
					<!--  Profile banner image end here -->
					<g:HTMLPanel styleName="{tocStyle.whitebg}">
						<g:HTMLPanel ui:field="marginDiv" styleName="{tocStyle.marginDiv}">
						    <g:HTMLPanel styleName="{tocStyle1.paginationBlock}">
							   <g:HTMLPanel styleName="col-md-6" ui:field="breadCrumbsPanel"><g:Anchor ui:field="mainTitle"/> > <g:Anchor ui:field="firstTitle"/> </g:HTMLPanel>
							   <g:HTMLPanel styleName="col-md-6 {tocStyle1.shareButton}">
							  		 <g:Button styleName="primary">Share </g:Button>
							   </g:HTMLPanel>
							</g:HTMLPanel>
							<g:Button ui:field="btnBackToPrevious" styleName="{tocStyle.btnBackToPrevious} backArrowButton" />
							<tbp:H3Panel styleName="{tocStyle.pageHeader}" ui:field="lblFolderTitle"> </tbp:H3Panel>
							<g:HTMLPanel ui:field="bigIdeasPanel">
								<p>
									<g:Label styleName="{tocStyle.lblFolderTitles}" text="{i18n.GL1731} {i18n.GL_SPL_SEMICOLON}" title="{i18n.GL1731}"></g:Label>
									<br> 
									<g:Label styleName="{tocStyle.lblFolderMetadata}" ui:field="lblBigIdeas"></g:Label>
									</br>
								</p>
							</g:HTMLPanel>
							<g:HTMLPanel ui:field="essentialPanel">
								<p>
									<g:Label  styleName="{tocStyle.lblFolderTitles}" text="{i18n.GL1732} {i18n.GL_SPL_SEMICOLON}" title="{i18n.GL1732}"></g:Label>
									<br> 
									<g:Label styleName="{tocStyle.lblFolderMetadata}" ui:field="lblEssentalQuestions"></g:Label>
									</br>
								</p>
							</g:HTMLPanel>
							<g:HTMLPanel ui:field="performancePanel">
								<p>
									<g:Label styleName="{tocStyle.lblFolderTitles}" text="{i18n.GL1733} {i18n.GL_SPL_SEMICOLON}" title="{i18n.GL1733}"></g:Label>
									<br> 
									<g:Label styleName="{tocStyle.lblFolderMetadata}" ui:field="lblPerformanceTasks"></g:Label>
									</br>
								</p>
							</g:HTMLPanel>
						<g:ScrollPanel ui:field="dropdownListContainerScrollPanel" styleName="{tocStyle1.dropdownListContainerScrollPanel}">
	            			<g:HTMLPanel ui:field="floderTreeContainer" styleName="{tocStyle1.folderStucture}">
			   				</g:HTMLPanel>
						</g:ScrollPanel>
				</g:HTMLPanel>
				</g:HTMLPanel>
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>