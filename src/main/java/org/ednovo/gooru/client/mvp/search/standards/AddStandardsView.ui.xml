<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:panel="urn:import:org.ednovo.gooru.client.ui"
	xmlns:uc="urn:import:org.ednovo.gooru.client.uc">
	<ui:style field="createCollectionStyle" src="CreateCollectionPopUp.css"/>

	<g:HTMLPanel styleName="{createCollectionStyle.shelfItemContentDiv}">
	<g:HTMLPanel>
		<g:HTMLPanel styleName="{createCollectionStyle.btnStandardsContainer}">
		<g:Button styleName="secondary {createCollectionStyle.btnStandardsStyle}" ui:field="commonStandards"></g:Button>
			</g:HTMLPanel>
			<g:HTMLPanel>
		<g:Button styleName="secondary {createCollectionStyle.btnStandardsStyle}" ui:field="texasKnowledge"></g:Button>
			</g:HTMLPanel>
			<g:HTMLPanel>
		<g:Button styleName="secondary {createCollectionStyle.btnStandardsStyle}" ui:field="ngss"></g:Button>
			</g:HTMLPanel>
			<g:HTMLPanel>
		<g:Button styleName="secondary {createCollectionStyle.btnStandardsStyle}"  ui:field="californiaStandards"></g:Button>
			</g:HTMLPanel>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="{createCollectionStyle.dropContainer}">
		<g:HTMLPanel styleName="{createCollectionStyle.dropOverContainer}" ui:field="levelOneStandards"/>
		<g:HTMLPanel styleName="{createCollectionStyle.dropOverContainerSecondlevel}" ui:field="levelTwoStandards"/>
		<g:HTMLPanel styleName="{createCollectionStyle.dropOverContainer}" ui:field="levelThreeStandards"/>
		<g:HTMLPanel styleName="{createCollectionStyle.dropOverContainerLastlevel}" ui:field="levelFourStandards"/>
		</g:HTMLPanel>
		<g:HTMLPanel styleName="addBtnOuterContainer"><g:Button styleName="primary {createCollectionStyle.addBtnStyle}" ui:field="addBtn">Add</g:Button></g:HTMLPanel>
		
	</g:HTMLPanel>
</ui:UiBinder>