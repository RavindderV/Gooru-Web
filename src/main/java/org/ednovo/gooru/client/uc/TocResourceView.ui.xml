<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:header="urn:import:org.ednovo.gooru.client.mvp.play.collection.header">
	<ui:style field= "playerStyle" src="resourcetocview.css"/>
	<g:FlowPanel styleName="{playerStyle.resource-container}">
		<g:FlowPanel styleName="{playerStyle.resource-thumbnail}"> 
			<g:Image styleName="{playerStyle.resource-image}" ui:field="resourceThumbnail"></g:Image>
			<g:Label styleName="{playerStyle.resource-type}" ui:field="resourceTypeImage"></g:Label>
		</g:FlowPanel>
		<g:HTMLPanel styleName="{playerStyle.nav-resource-title}" ui:field="resourceTitle"></g:HTMLPanel>
	</g:FlowPanel>
</ui:UiBinder>