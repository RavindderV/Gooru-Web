<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gc="urn:import:org.ednovo.gooru.client.mvp.search" xmlns:sg="urn:import:org.ednovo.gooru.client.mvp.home" >
	<ui:style type="org.ednovo.gooru.client.mvp.search.SearchRootView.Style">
		.searchWrapper {
			/*min-height: 383px;*/
			margin: 0 auto;
			/* width: 960px; */
		}
		
		.searchTotalContainer {
			/*position: relative;*/
			height: auto !important;
			margin-top: 20px;
		}
		
		.contentDiv {
			margin: 2px auto 0;
			width: 723px;
		}
		
		.PageNotify {
			margin: 0px 0 0 207px;
			width: 596px;
			display: inline-block;

		}
		.notifyLink {
			color: #4E9746;
			cursor: pointer;
			float: left;
			margin: 0 2px;
			padding-bottom: 2px;
		}
		<!-- .notifyLink:HOVER {
		    border-bottom: 2px solid #A6CAA2;
		    cursor: pointer;
		    padding-bottom: 2px;
		    color: #4E9746;
		    text-decoration:none;
		} -->
		.notifyLinks {
		    color: #4E9746 !important;
		    cursor: default;
		    float: left;
		    font-size: 15px;
		    font-weight: bold;
		    margin: -3px 5px 0 6px;
		}
		.active {
			margin-left: 5px;
			padding: 7px 31px;
			background: url(../images/resource-type-icons.png) no-repeat #efefef;
			background-position: 2px -24px !important;
		}

		.resourceSearchButtonImage{
			margin-left: 5px;
			padding: 7px 31px !important;
			background: url(../images/resource-type-icons.png) no-repeat transparent !important;
			background-position: 2px -24px !important;
			background-position: 0px -27px !important;
		}

		.bodyHeight {
			height: auto !important;
		}
		.panelHeight{
			overflow-y: auto;
			overflow-x: hidden;
			width: 63.75%;
			padding: 0px 0px 0px 12%;
		}
		.searchResultPanel {
		    clear: both;
		    height: 34px;
		   <!--  margin: 20px auto 0; -->
		    width: 796px;
		}
		
		.searchResult {
			color: #515151;
			<!-- float: left; -->
			margin: 32px 0 0 204px;
			display: inline-block;
			padding-bottom: 10px;
			
		}
		
		.notifyColorLink {
			color: #222222;
		}
		.loadingImageContainer
		{
			 background: url("images/core/search.gif");
 	     	width: 130px;
         	height: 100px;
         	margin: 0 auto;
         	margin-top: 110px;
		}
		

		.secondaryIC{
			color: #515151;
			padding: 6px 9px;
			border-radius: 4px;
			background: #f7f7f7;
			background: -webkit-gradient(linear, 0 0, 0 100%, from(#ffffff), to(#f7f7f7));
			background: -webkit-linear-gradient(top, #ffffff, #f7f7f7);
			background: -moz-linear-gradient(top, #ffffff, #f7f7f7);
			background: -ms-linear-gradient(top, #ffffff, #f7f7f7);
			background: -o-linear-gradient(top, #ffffff, #f7f7f7);
			border: 1px solid #ddd;
			font-size:small;
			font-family:Arial, Helvetica, sans-serif;
			cursor:pointer;
		}

		.secondaryIC:hover{
			color: #515151;
			background: #f7f7f7;
			background: -webkit-gradient(linear, 0 0, 0 100%, from(#f7f7f7), to(#ffffff));
			background: -webkit-linear-gradient(top, #f7f7f7, #ffffff);
			background: -moz-linear-gradient(top, #f7f7f7, #ffffff);
			background: -ms-linear-gradient(top, #f7f7f7, #ffffff);
			background: -o-linear-gradient(top, #f7f7f7, #ffffff);
		}

		.secondaryIC img{
			vertical-align:middle;
			width:22px;
			margin-right:5px;
		}

	</ui:style>
	<g:HTMLPanel styleName="{style.searchTotalContainer}">
		<!-- <gc:SearchBarVc ui:field="searchBarVc" /> -->
		<g:FlowPanel styleName="" ui:field="panelSearchPage">
			<g:FlowPanel styleName="{style.searchResultPanel}">
				<g:HTML styleName="{style.searchResult}" ui:field="queriedTextHtml" />
				<g:FlowPanel styleName="{style.PageNotify}" ui:field="flowpanel">
					<g:HTMLPanel ui:field="contentpanel"></g:HTMLPanel>
					<g:Button styleName="secondary {style.notifyLink}" ui:field="resourceSearchBtn"/>
					<!-- <g:Label text="|" styleName="{style.notifyLinks}" /> -->
					<g:Button styleName="secondary {style.notifyLink}" ui:field="collectionSearchBtn"/>
				</g:FlowPanel>
			</g:FlowPanel>
			<g:SimplePanel styleName="{style.searchWrapper}"  ui:field="searchWrapperSimPanel"/>
			<g:SimplePanel styleName="searchResults" ui:field="shelfTabSimPanel"/>
			<g:Label ui:field="lodingImage" styleName="{style.loadingImageContainer}" />
			<sg:FooterUc />
		</g:FlowPanel>
	</g:HTMLPanel>

</ui:UiBinder> 
