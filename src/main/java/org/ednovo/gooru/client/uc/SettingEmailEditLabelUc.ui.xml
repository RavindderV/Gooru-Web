<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="org.ednovo.gooru.client.uc.UcCBundle" field="res" />
<g:HTMLPanel>
	<g:FocusPanel ui:field="focusPanel" styleName="{res.css.editableFieldSetting}">
		<g:DeckPanel ui:field="deckPanel">
			<g:Label ui:field="editLabel" styleName="{res.css.editableUserSettingField}"/>
			<g:TextBox styleName="edit" maxLength="25" ui:field="editTextBox" />
		</g:DeckPanel>
	</g:FocusPanel>
	<g:Label ui:field="errorLabel" styleName="{res.css.errorValidationEmailForSetting}"/>
	</g:HTMLPanel>
</ui:UiBinder> 