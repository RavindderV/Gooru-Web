<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:header="urn:import:org.ednovo.gooru.client.mvp.play.collection.header">
	<ui:style field= "collectionSummaryPageStyle" src="collectionend.css"/>
	<g:HTMLPanel styleName="{collectionSummaryPageStyle.collectionxPlayerSummaryWrapper}">	   
  			<div class="{collectionSummaryPageStyle.collectionPlayerReplayContainer}">
   			<div class="{collectionSummaryPageStyle.collectionxPlayerSummaryTitle}">
      			<div class="{collectionSummaryPageStyle.collectionxPlayerSummaryTitleText}">You've reached the end of this collection!</div>
   			</div>
			<g:HTMLPanel ui:field="collectionReplayButtonImage" styleName="{collectionSummaryPageStyle.collectionxPlayerRefreshContainer}">
     			<div class="{collectionSummaryPageStyle.collectionxPlayerRefreshImage}">
		    		<g:Label styleName="{collectionSummaryPageStyle.collectionxPlayerRefreshText}">Replay</g:Label>
        			<g:Label></g:Label>
     			</div>
			</g:HTMLPanel>
		</div>
		<g:HTMLPanel styleName="{collectionSummaryPageStyle.collectionxPlayerShareContainer}">
			<g:HTMLPanel  styleName="{collectionSummaryPageStyle.collectionPlayerSummaryShareContainer}">
				<g:HTMLPanel ui:field="collectionSummaryShareButtonImage">
   					 <g:Label styleName="{collectionSummaryPageStyle.collection-share-view-text}" >Share this Collection</g:Label>
	    			 <div class="{collectionSummaryPageStyle.collectionPlayerSumShareContainer}">
        				<g:Label styleName="{collectionSummaryPageStyle.collectionxPlayerShareText}"></g:Label>
       					<g:TextBox styleName="{collectionSummaryPageStyle.collectionPlayerSummaryShareTextBox}" ui:field="collectionPlayerSummaryShareTextBox"></g:TextBox>
       				</div>
         				<div class="{collectionSummaryPageStyle.collectionPlayerInputSeparatorLine}"></div>	
	   				<g:HTMLPanel ui:field="ftmSummaryPageContainer" styleName="{collectionSummaryPageStyle.ftmSummaryPageContainer}"></g:HTMLPanel> 
	   			</g:HTMLPanel>
 				 </g:HTMLPanel>
 			 </g:HTMLPanel>
		<!-- Inline CSS for print/pdf/email functionality -->
  			<g:HTMLPanel ui:field="printElementContainer" styleName="{collectionSummaryPageStyle.printElementContainer}">
   			<g:HTMLPanel styleName="{collectionSummaryPageStyle.cpIconsContainer}">
   				<g:Label styleName="{collectionSummaryPageStyle.cpSummaryText}">Save this summary?</g:Label>
   				<g:Label ui:field="emailButton" styleName="{collectionSummaryPageStyle.collection-email-icon}"></g:Label>
   				<g:Label ui:field="printButton" styleName="{collectionSummaryPageStyle.collection-print-icon}"></g:Label>
   			</g:HTMLPanel>
   			<g:HTMLPanel ui:field="summaryQuestionsContainer">
   			</g:HTMLPanel>
  		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder>