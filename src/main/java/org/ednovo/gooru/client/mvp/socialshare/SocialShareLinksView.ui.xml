<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with type="org.ednovo.gooru.client.mvp.shelf.ShelfCBundle"
		field="res" />
	<g:FlowPanel styleName="{res.css.shareLink}" ui:field="shareLinkFlwPl">
		<g:Label text="Share this with others by copying and pasting these links" ui:field="shareLbl"/>
		<g:FlowPanel styleName="{res.css.shareLinkBoxContainer}">
			<g:TextArea styleName="{res.css.shareLinkBox}" ui:field="shareLinkTxtBox" />
		</g:FlowPanel>
		<g:Label styleName="{res.css.shareLinkSwitchUrl}" ui:field="swithUrlLbl" text="Switch to Bit.ly" />
		<g:Label styleName="{res.css.shareLinkSwitchEmbed}" ui:field="swithToEmbedLbl" text="Switch to embed code" />
	</g:FlowPanel>
</ui:UiBinder> 