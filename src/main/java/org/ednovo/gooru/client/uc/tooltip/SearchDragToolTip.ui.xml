<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:panel="urn:import:org.ednovo.gooru.client.ui">
	<ui:style field="toolTip" src="ToolTip.css" />
		
	<panel:HTMLEventPanel styleName="{toolTip.searchDragPopupContainer}">
		<g:HTMLPanel styleName="{toolTip.searchDragInnerPopup}">
			<g:HTMLPanel>
			    <g:Image ui:field="searchDragToolTipImage" />
			    <g:HTMLPanel styleName="{toolTip.searchDragDescContainer}">
					<g:Label styleName="{toolTip.searchDragDesc}" ui:field="desLbl"></g:Label>
				</g:HTMLPanel>	
			</g:HTMLPanel>
		</g:HTMLPanel>
	</panel:HTMLEventPanel>

</ui:UiBinder> 