<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:gg="urn:import:org.ednovo.gooru.client.uc" >
	<ui:with type="org.ednovo.gooru.client.mvp.home.register.RegisterCBundle" field="res" />
	<g:FlowPanel styleName="{res.css.childRegisterContent}" height="150px">
		<g:Label styleName="{res.css.childAccountText1}" text="Congrulations! Your child's account has been successfully created. A confirmation email has been send to your email, which contains details about your child account. " />
		<g:Label styleName="{res.css.childAccountText2}" text="We're excited for your child to start taking advantage of Gooru learning resources!" />
		<gg:BlueButtonUc text="Ok" styleName="{res.css.childConfirmButton}" ui:field="confirmationBtnUc"/>
	</g:FlowPanel>	
	</ui:UiBinder> 