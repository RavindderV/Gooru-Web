<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style field="foldersWelcomepageToolTip" src="FolderWelcomepageTooltip.css" />
	<g:HTMLPanel>
		<g:HTMLPanel styleName="{foldersWelcomepageToolTip.popup}">
			<g:HTMLPanel styleName="{foldersWelcomepageToolTip.standardPopup}">
				<g:HTMLPanel styleName="{foldersWelcomepageToolTip.bubbleArrow}"></g:HTMLPanel>
				<g:HTMLPanel styleName="{foldersWelcomepageToolTip.standardsInner}">
					<g:Label ui:field="mainHeadTitle" styleName="{foldersWelcomepageToolTip.title}"></g:Label>
					<g:HTMLPanel ui:field="descTextLineOne"	styleName="{foldersWelcomepageToolTip.desc}"><g:Label ui:field="gerStartedLbl"></g:Label> 
						<g:HTMLPanel ui:field="descTextLineTwo"	styleName="{foldersWelcomepageToolTip.descInline}"></g:HTMLPanel>
					</g:HTMLPanel>
				</g:HTMLPanel>
			</g:HTMLPanel>
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 